/* union for signal handlers */
union __sigaction_u {
//CMNT: c_keyword possible_pointer remove_casts ^|        void (*__sa_handler)(int); 
 ( _ _sa_handler);
//CMNT: c_keyword possible_pointer ^|        void (*__sa_sigaction)(int, struct __siginfo *, 
 ( _ _sa_sigaction)(, struct __siginfo  , 
//CMNT: c_keyword possible_pointer ^|                void *); 
  ) ;
};

/* Signal vector template for Kernel user boundary */
struct __sigaction {
 union __sigaction_u __sigaction_u; /* signal handler */
//CMNT: c_keyword possible_pointer ^|        void (*sa_tramp)(void *, int, int, siginfo_t *, void *); 
 ( s a_tramp)(  ,  , , siginfo_t  ,  ) ;
 sigset_t sa_mask; /* signal mask to apply */
//CMNT: c_keyword ^|        int sa_flags; /* see signal options below */ 
  sa_flags; /* see signal options below */
};

/*
//CMNT: possible_pointer ^|        * Signal vector "template" used in sigaction call. 
    Signal vector "template" used in sigaction call.
 */
struct sigaction {
 union __sigaction_u __sigaction_u; /* signal handler */
 sigset_t sa_mask; /* signal mask to apply */
//CMNT: c_keyword ^|        int sa_flags; /* see signal options below */ 
  sa_flags; /* see signal options below */
};



