/*
//CMNT: possible_pointer ^|        * "Local" flags - dumping ground for other state 
    "Local" flags - dumping ground for other state
 *
//CMNT: possible_pointer ^|        * Warning: some flags in this structure begin with 
    Warning: some flags in this structure begin with
//CMNT: possible_pointer ^|        * the letter "I" and look like they belong in the 
    the letter "I" and look like they belong in the
//CMNT: possible_pointer ^|        * input flag. 
    input flag.
 */
      //     # 265 "/usr/include/sys/termios.h" 3 4
//CMNT: c_keyword ^|       typedef unsigned long tcflag_t; 
typedef  long tcflag_t;
//CMNT: c_keyword ^|       typedef unsigned char cc_t; 
typedef   cc_t;
//CMNT: c_keyword ^|       typedef unsigned long speed_t; 
typedef  long speed_t;

struct termios {
 tcflag_t c_iflag; /* input flags */
 tcflag_t c_oflag; /* output flags */
 tcflag_t c_cflag; /* control flags */
 tcflag_t c_lflag; /* local flags */
//CMNT: c_keyword ^|        cc_t c_cc[20]; /* control chars */ 
 cc_t c_cc[20]; /* control s */
 speed_t c_ispeed; /* input speed */
 speed_t c_ospeed; /* output speed */
};


