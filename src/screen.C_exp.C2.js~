template<typename signature>
struct callback;

template<class R>
struct callback<R ()>
{
//CMNT: c_keyword possible_pointer ^|         typedef R (*ptr_type)(void *self); 
  typedef R ( p tr_type)(  s elf);

//CMNT: possible_pointer ^|         template<class K, R (K::*method)()> 
  template<class K, R (K:: m ethod)()>
//CMNT: c_keyword possible_pointer ^|         void set (K *object){ 
 set (K  o bject){
    self = object;
    func = thunk<K, method>;
  }

//CMNT: c_keyword ^|         R call () const 
  R call () 
  {
    return func (self);
  }

//CMNT: c_keyword ^|         R operator ()() const 
  R operator ()() 
  {
    return call ();
  }

private:

//CMNT: c_keyword possible_pointer ^|         void *self; 
  s elf;
  ptr_type func;

//CMNT: possible_pointer ^|         template<class klass, R (klass::*method)()> 
  template<class klass, R (klass:: m ethod)()>
//CMNT: c_keyword possible_pointer ^|         static R thunk (void *self){ 
   R thunk (  s elf){
//CMNT: c_keyword possible_pointer ^|           klass *obj = static_cast<klass *>(self); 
    klass  o bj = _cast<klass  > (self);
//CMNT: possible_pointer ^|           return (obj->*method) (); 
    return (obj-> m ethod) ();
  }
};

template<class R, class A1>
struct callback<R (A1)>
{
