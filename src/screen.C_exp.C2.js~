//CMNT: possible_pointer ^|             XrmRepresentation* /* type */, 
      XrmRepresentation   /* type */,
//CMNT: possible_pointer ^|             XrmValue* /* value */, 
      XrmValue   /* value */,
//CMNT: c_keyword ^|             XPointer /* closure */ 
      XPoer /* closure */
      ) /* proc */,
//CMNT: c_keyword ^|           XPointer /* closure */ 
    XPoer /* closure */
);

//CMNT: c_keyword possible_pointer ^|       extern const char *XrmLocaleOfDatabase( 
extern    X rmLocaleOfDatabase(
    XrmDatabase /* database */
);


//CMNT: possible_pointer ^|       /**************************************************************** 
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * *
 *
//CMNT: possible_pointer ^|        * Command line option mapping to resource entries 
    Command line option mapping to resource entries
 *
//CMNT: possible_pointer ^|        ****************************************************************/ 
  * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * **/

typedef enum {
    XrmoptionNoArg, /* Value is specified in OptionDescRec.value	    */
    XrmoptionIsArg, /* Value is the option string itself		    */
//CMNT: c_keyword ^|           XrmoptionStickyArg, /* Value is characters immediately following option */ 
    XrmoptionStickyArg, /* Value is acters immediately following option */
    XrmoptionSepArg, /* Value is next argument in argv		    */
    XrmoptionResArg, /* Resource and value in next argument in argv      */
    XrmoptionSkipArg, /* Ignore this option and the next argument in argv */
    XrmoptionSkipLine, /* Ignore this option and the rest of argv	    */
    XrmoptionSkipNArgs /* Ignore this option and the next 
			   OptionDescRes.value arguments in argv */
} XrmOptionKind;

typedef struct {
//CMNT: c_keyword possible_pointer ^|           char *option; /* Option abbreviation in argv	    */ 
      o ption; /* Option abbreviation in argv	    */
//CMNT: c_keyword possible_pointer ^|           char *specifier; /* Resource specifier		    */ 
      s pecifier; /* Resource specifier		    */
    XrmOptionKind argKind; /* Which style of option it is	    */
//CMNT: c_keyword ^|           XPointer value; /* Value to provide if XrmoptionNoArg   */ 
    XPoer value; /* Value to provide if XrmoptionNoArg   */
//CMNT: possible_pointer ^|       } XrmOptionDescRec, *XrmOptionDescList; 
} XrmOptionDescRec,  X rmOptionDescList;


//CMNT: c_keyword ^|       extern void XrmParseCommand( 
extern XrmParseCommand(
//CMNT: possible_pointer ^|           XrmDatabase* /* database */, 
    XrmDatabase   /* database */,
    XrmOptionDescList /* table */,
//CMNT: c_keyword ^|           int /* table_count */, 
     /* table_count */,
//CMNT: c_keyword possible_pointer ^|           const char* /* name */, 
        /* name */,
//CMNT: c_keyword possible_pointer ^|           int* /* argc_in_out */, 
       /* argc_in_out */,
//CMNT: c_keyword possible_pointer ^|           char** /* argv_in_out */ 
     *  /* argv_in_out */
);

}


