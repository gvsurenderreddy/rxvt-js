/* from ISO/IEC 988:1999 spec */

//CMNT: c_keyword ^|       /* 7.18.1.1 Exact-width integer types */ 
/* 7.18.1.1 Exact-width eger types */
//CMNT: c_keyword ^|             //     # 40 "/usr/lib/gcc/i686-apple-darwin9/4.0.1/include/stdint.h" 3 4 
      //     # 40 "/usr/lib/gcc/i686-apple-darwin9/4.0.1/include/std.h" 3 4
//CMNT: c_keyword ^|       typedef unsigned char uint8_t; 
typedef   u8_t;




//CMNT: c_keyword ^|       typedef unsigned short uint16_t; 
typedef   u16_t;




//CMNT: c_keyword ^|       typedef unsigned int uint32_t; 
typedef   u32_t;




//CMNT: c_keyword ^|       typedef unsigned long long uint64_t; 
typedef  long long u64_t;


//CMNT: c_keyword ^|       /* 7.18.1.2 Minimum-width integer types */ 
/* 7.18.1.2 Minimum-width eger types */
//CMNT: c_keyword ^|       typedef int8_t int_least8_t; 
typedef 8_t _least8_t;
//CMNT: c_keyword ^|       typedef int16_t int_least16_t; 
typedef 16_t _least16_t;
//CMNT: c_keyword ^|       typedef int32_t int_least32_t; 
typedef 32_t _least32_t;
//CMNT: c_keyword ^|       typedef int64_t int_least64_t; 
typedef 64_t _least64_t;
//CMNT: c_keyword ^|       typedef uint8_t uint_least8_t; 
typedef u8_t u_least8_t;
//CMNT: c_keyword ^|       typedef uint16_t uint_least16_t; 
typedef u16_t u_least16_t;
//CMNT: c_keyword ^|       typedef uint32_t uint_least32_t; 
typedef u32_t u_least32_t;
//CMNT: c_keyword ^|       typedef uint64_t uint_least64_t; 
typedef u64_t u_least64_t;


//CMNT: c_keyword ^|       /* 7.18.1.3 Fastest-width integer types */ 
/* 7.18.1.3 Fastest-width eger types */
//CMNT: c_keyword ^|       typedef int8_t int_fast8_t; 
typedef 8_t _fast8_t;
//CMNT: c_keyword ^|       typedef int16_t int_fast16_t; 
typedef 16_t _fast16_t;
//CMNT: c_keyword ^|       typedef int32_t int_fast32_t; 
typedef 32_t _fast32_t;
//CMNT: c_keyword ^|       typedef int64_t int_fast64_t; 
typedef 64_t _fast64_t;
//CMNT: c_keyword ^|       typedef uint8_t uint_fast8_t; 
typedef u8_t u_fast8_t;
//CMNT: c_keyword ^|       typedef uint16_t uint_fast16_t; 
typedef u16_t u_fast16_t;
//CMNT: c_keyword ^|       typedef uint32_t uint_fast32_t; 
typedef u32_t u_fast32_t;
//CMNT: c_keyword ^|       typedef uint64_t uint_fast64_t; 
typedef u64_t u_fast64_t;


//CMNT: c_keyword ^|       /* 7.18.1.4 Integer types capable of holding object pointers */ 
