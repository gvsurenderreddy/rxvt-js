// increases code size unless -fno-enforce-eh-specs
      //     # 43 "rxvtutil.h"
extern class byteorder {
//CMNT: c_keyword ^|         static unsigned int e; // at least 32 bits 
     e; // at least 32 bits
public:
  byteorder ();

//CMNT: c_keyword ^|         static bool big_endian () { return e == 0x11223344; }; 
   bool big_endian () { return e == 0x11223344; };
//CMNT: c_keyword ^|         static bool network () { return e == 0x11223344; }; 
   bool network () { return e == 0x11223344; };
//CMNT: c_keyword ^|         static bool little_endian () { return e == 0x44332211; }; 
   bool little_endian () { return e == 0x44332211; };
//CMNT: c_keyword ^|         static bool vax () { return e == 0x44332211; }; 
   bool vax () { return e == 0x44332211; };
} byteorder;

// various utility functions
//CMNT: c_keyword ^|       template<typename T, typename U> static inline T min (T a, U b) { return a < (T)b ? a : (T)b; } 
template<typename T, typename U>  inline T min (T a, U b) { return a < (T)b ? a : (T)b; }
//CMNT: c_keyword ^|       template<typename T, typename U> static inline void min_it (T &a, U b) { a = a < (T)b ? a : (T)b; } 
template<typename T, typename U>  inline min_it (T &a, U b) { a = a < (T)b ? a : (T)b; }
//CMNT: c_keyword ^|       template<typename T, typename U> static inline T max (T a, U b) { return a > (T)b ? a : (T)b; } 
template<typename T, typename U>  inline T max (T a, U b) { return a > (T)b ? a : (T)b; }
//CMNT: c_keyword ^|       template<typename T, typename U> static inline void max_it (T &a, U b) { a = a > (T)b ? a : (T)b; } 
template<typename T, typename U>  inline max_it (T &a, U b) { a = a > (T)b ? a : (T)b; }

//CMNT: c_keyword ^|       template<typename T, typename U, typename V> static inline T clamp (T v, U a, V b) { return v < (T)a ? a : v >(T)b ? b : v; } 
template<typename T, typename U, typename V>  inline T clamp (T v, U a, V b) { return v < (T)a ? a : v >(T)b ? b : v; }
//CMNT: c_keyword ^|       template<typename T, typename U, typename V> static inline void clamp_it (T &v, U a, V b) { v = v < (T)a ? a : v >(T)b ? b : v; } 
template<typename T, typename U, typename V>  inline clamp_it (T &v, U a, V b) { v = v < (T)a ? a : v >(T)b ? b : v; }

//CMNT: c_keyword ^|       template<typename T, typename U> static inline void swap (T& a, U& b) { T t=a; a=(T)b; b=(U)t; } 
template<typename T, typename U>  inline swap (T& a, U& b) { T t=a; a=(T)b; b=(U)t; }

//CMNT: c_keyword possible_pointer ^|       template<typename T> static inline T squared_diff (T a, T b) { return (a-b)*(a-b); } 
template<typename T>  inline T squared_diff (T a, T b) { return (a-b) ( a-b); }

//CMNT: c_keyword ^|       // linear interpolation 
// linear erpolation
template<typename T, typename U, typename P>
//CMNT: c_keyword ^|       static inline 
 inline
T lerp (T a, U b, P p){
//CMNT: possible_pointer ^|         return (long(a) * long(100 - p) + long(b) * long(p) + 50) / 100; 
  return (long(a)    long(100 - p) + long(b)    long(p) + 50) / 100;
}

template <typename I, typename T>
//CMNT: c_keyword ^|       I find (I first, I last, const T& value){ 
I find (I first, I last,  T& value){
//CMNT: possible_pointer ^|         while (first != last && *first != value) 
  while (first != last &&  f irst != value)
    ++first;

  return first;
}

// return a very temporary (and never deallocated) buffer. keep small.
//CMNT: c_keyword possible_pointer ^|       void *rxvt_temp_buf (int len); 
  r xvt_temp_buf ( len);

template<typename T>
//CMNT: c_keyword ^|       static inline T * 
 inline T *
//CMNT: c_keyword ^|       rxvt_temp_buf (int len){ 
rxvt_temp_buf ( len){
//CMNT: possible_pointer ^|         return (T *)rxvt_temp_buf (len * sizeof (T)); 
  return (T  ) rxvt_temp_buf (len    sizeof (T));
}

