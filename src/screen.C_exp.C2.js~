      //     # 413 "/usr/include/mach/i386/_structs.h" 3 4
struct __darwin_x86_float_state64
{
//CMNT: c_keyword ^|        int __fpu_reserved[2]; 
  __fpu_reserved[2];
 struct __darwin_fp_control __fpu_fcw; /* x87 FPU control word */
 struct __darwin_fp_status __fpu_fsw; /* x87 FPU status word */
//CMNT: c_keyword ^|        __uint8_t __fpu_ftw; /* x87 FPU tag word */ 
 __u8_t __fpu_ftw; /* x87 FPU tag word */
//CMNT: c_keyword ^|        __uint8_t __fpu_rsrv1; /* reserved */ 
 __u8_t __fpu_rsrv1; /* reserved */
//CMNT: c_keyword ^|        __uint16_t __fpu_fop; /* x87 FPU Opcode */ 
 __u16_t __fpu_fop; /* x87 FPU Opcode */

//CMNT: c_keyword ^|        /* x87 FPU Instruction Pointer */ 
 /* x87 FPU Instruction Poer */
//CMNT: c_keyword ^|        __uint32_t __fpu_ip; /* offset */ 
 __u32_t __fpu_ip; /* offset */
//CMNT: c_keyword ^|        __uint16_t __fpu_cs; /* Selector */ 
 __u16_t __fpu_cs; /* Selector */

//CMNT: c_keyword ^|        __uint16_t __fpu_rsrv2; /* reserved */ 
 __u16_t __fpu_rsrv2; /* reserved */

