      //     # 65 "/usr/include/limits.h" 2 3 4
      //     # 1 "/usr/include/sys/syslimits.h" 1 3 4
/*
//CMNT: possible_pointer ^|        * Copyright (c) 2000 Apple Computer, Inc. All rights reserved. 
