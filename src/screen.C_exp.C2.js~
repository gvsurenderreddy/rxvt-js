typedef struct __siginfo {
//CMNT: c_keyword ^|        int si_signo; /* signal number */ 
  si_signo; /* signal number */
//CMNT: c_keyword ^|        int si_errno; /* errno association */ 
  si_errno; /* errno association */
//CMNT: c_keyword ^|        int si_code; /* signal code */ 
  si_code; /* signal code */
 pid_t si_pid; /* sending process */
 uid_t si_uid; /* sender's ruid */
//CMNT: c_keyword ^|        int si_status; /* exit value */ 
  si_status; /* exit value */
//CMNT: c_keyword possible_pointer ^|        void *si_addr; /* faulting instruction */ 
  s i_addr; /* faulting instruction */
 union sigval si_value; /* signal value */
 long si_band; /* band event for SIGPOLL */
//CMNT: c_keyword ^|        unsigned long __pad[7]; /* Reserved for Future Use */ 
  long __pad[7]; /* Reserved for Future Use */
} siginfo_t;


