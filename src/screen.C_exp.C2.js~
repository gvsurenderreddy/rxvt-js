      //     # 52 "/usr/include/c++/4.0.0/cstring" 2 3

// Get rid of those macros defined in <string.h> in lieu of real functions.
      //     # 77 "/usr/include/c++/4.0.0/cstring" 3
namespace std
{
  using ::memcpy;
  using ::memmove;
  using ::strcpy;
  using ::strncpy;
  using ::strcat;
  using ::strncat;
  using ::memcmp;
  using ::strcmp;
  using ::strcoll;
  using ::strncmp;
  using ::strxfrm;
  using ::strcspn;
  using ::strspn;
  using ::strtok;
  using ::memset;
  using ::strerror;
  using ::strlen;

  using ::memchr;

//CMNT: c_keyword ^|         inline void* 
  inline*
//CMNT: c_keyword possible_pointer ^|         memchr(void* __p, int __c, size_t __n){ return memchr(const_cast<const void*>(__p), __c, __n); } 
  memchr(   __p,  __c, size_t __n){ return memchr(_cast< > (__p), __c, __n); }

  using ::strchr;

//CMNT: c_keyword ^|         inline char* 
  inline *
//CMNT: c_keyword possible_pointer ^|         strchr(char* __s1, int __n){ return __builtin_strchr(const_cast<const char*>(__s1), __n); } 
  strchr(   __s1,  __n){ return __builtin_strchr(_cast<  > (__s1), __n); }

  using ::strpbrk;

//CMNT: c_keyword ^|         inline char* 
  inline *
//CMNT: c_keyword possible_pointer ^|         strpbrk(char* __s1, const char* __s2){ return __builtin_strpbrk(const_cast<const char*>(__s1), __s2); } 
  strpbrk(   __s1,     __s2){ return __builtin_strpbrk(_cast<  > (__s1), __s2); }

  using ::strrchr;

//CMNT: c_keyword ^|         inline char* 
  inline *
//CMNT: c_keyword possible_pointer ^|         strrchr(char* __s1, int __n){ return __builtin_strrchr(const_cast<const char*>(__s1), __n); } 
  strrchr(   __s1,  __n){ return __builtin_strrchr(_cast<  > (__s1), __n); }

  using ::strstr;

//CMNT: c_keyword ^|         inline char* 
  inline *
//CMNT: c_keyword possible_pointer ^|         strstr(char* __s1, const char* __s2){ return __builtin_strstr(const_cast<const char*>(__s1), __s2); } 
  strstr(   __s1,     __s2){ return __builtin_strstr(_cast<  > (__s1), __s2); }
}
