template<class watcher>
//CMNT: possible_pointer ^|       struct event_vec : vector<watcher *> 
struct event_vec : vector<watcher  > 
{
//CMNT: c_keyword ^|         void erase_unordered (unsigned int pos){ 
 erase_unordered (  pos){
//CMNT: possible_pointer ^|           watcher *w = (*this)[this->size () - 1]; 
    watcher  w  = ( t his)[this->size () - 1];
    this->pop_back ();

    if (!this->empty ())
//CMNT: possible_pointer ^|             if (((*this)[pos] = w)) // '=' is correct! 
      if ((( t his)[pos] = w)) // '=' is correct!
        w->active = pos + 1;
  }
};

struct rxvt_watcher
{
//CMNT: c_keyword ^|         int active; /* 0 == inactive, else index into respective vector */ 
   active; /* 0 == inactive, else index o respective vector */

  bool is_active () { return active; }

  rxvt_watcher () : active (0) { }
};

struct refcounted
{
//CMNT: c_keyword ^|         int referenced; 
   referenced;
//CMNT: c_keyword possible_pointer ^|         char *id; 
    i d;

//CMNT: c_keyword possible_pointer ^|         refcounted (const char *id); 
  refcounted (   i d);
  bool ref_init () { return false; }
//CMNT: c_keyword ^|         void ref_next () { } 
 ref_next () { }
  ~refcounted ();
};

template<class T>
//CMNT: possible_pointer ^|       struct refcache : vector<T *> 
struct refcache : vector<T  > 
{
//CMNT: c_keyword possible_pointer ^|         T *get (const char *id); 
  T  g et (   i d);
//CMNT: c_keyword possible_pointer ^|         void put (T *obj); 
 put (T  o bj);
//CMNT: c_keyword ^|         void clear (); 
 clear ();

  ~refcache (){
    clear ();
  }
};

