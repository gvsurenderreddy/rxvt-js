// temporarily replace the process environment
//CMNT: c_keyword possible_pointer ^|       extern char **environ; 
extern   * environ;
//CMNT: c_keyword possible_pointer ^|       extern char **rxvt_environ; // the original environ pointer 
extern   * rxvt_environ; // the original environ poer

//CMNT: c_keyword ^|       static inline void 
 inline
//CMNT: possible_pointer ^|       set_environ (stringvec *envv){ 
set_environ (stringvec  e nvv){



  if (envv)

//CMNT: c_keyword possible_pointer ^|           environ = (char **)envv->begin (); 
    environ = (  * )envv->begin ();
}

//CMNT: c_keyword ^|       static inline void 
 inline
//CMNT: c_keyword possible_pointer ^|       set_environ (char **envv){ 
set_environ (  * envv){



  if (envv)

    environ = envv;
}

