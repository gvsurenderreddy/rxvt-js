typedef __darwin_ct_rune_t ct_rune_t;




typedef __darwin_rune_t rune_t;
      //     # 70 "/usr/include/runetype.h" 3 4
//CMNT: c_keyword ^|       typedef __darwin_wint_t wint_t; 
typedef __darwin_w_t w_t;







/*
//CMNT: possible_pointer ^|        * The lower 8 bits of runetype[] contain the digit value of the rune. 
    The lower 8 bits of runetype[] contain the digit value of the rune.
 */
typedef struct {
 __darwin_rune_t __min; /* First rune of the range */
 __darwin_rune_t __max; /* Last rune (inclusive) of the range */
 __darwin_rune_t __map; /* What first maps to in maps */
//CMNT: c_keyword possible_pointer ^|        __uint32_t *__types; /* Array of types in range */ 
 __u32_t  _ _types; /* Array of types in range */
} _RuneEntry;

typedef struct {
//CMNT: c_keyword ^|        int __nranges; /* Number of ranges stored */ 
  __nranges; /* Number of ranges stored */
//CMNT: c_keyword possible_pointer ^|        _RuneEntry *__ranges; /* Pointer to the ranges */ 
 _RuneEntry  _ _ranges; /* Poer to the ranges */
} _RuneRange;

typedef struct {
//CMNT: c_keyword ^|        char __name[14]; /* CHARCLASS_NAME_MAX = 14 */ 
  __name[14]; /* CHARCLASS_NAME_MAX = 14 */
//CMNT: c_keyword ^|        __uint32_t __mask; /* charclass mask */ 
 __u32_t __mask; /* class mask */
} _RuneCharClass;

typedef struct {
//CMNT: c_keyword ^|        char __magic[8]; /* Magic saying what version we are */ 
  __magic[8]; /* Magic saying what version we are */
//CMNT: c_keyword ^|        char __encoding[32]; /* ASCII name of this encoding */ 
  __encoding[32]; /* ASCII name of this encoding */

//CMNT: c_keyword possible_pointer ^|        __darwin_rune_t (*__sgetrune)(const char *, __darwin_size_t, char const **); 
 __darwin_rune_t ( _ _sgetrune)(   ,  __darwin_size_t,    * );
//CMNT: c_keyword possible_pointer ^|        int (*__sputrune)(__darwin_rune_t, char *, __darwin_size_t, char **); 
  ( _ _sputrune)(__darwin_rune_t,   ,  __darwin_size_t,   * );
 __darwin_rune_t __invalid_rune;

//CMNT: c_keyword ^|        __uint32_t __runetype[(1 <<8 ) /* Must be a power of 2 */]; 
 __u32_t __runetype[(1 <<8 ) /* Must be a power of 2 */];
 __darwin_rune_t __maplower[(1 <<8 ) /* Must be a power of 2 */];
 __darwin_rune_t __mapupper[(1 <<8 ) /* Must be a power of 2 */];

 /*
//CMNT: possible_pointer ^|       	 * The following are to deal with Runes larger than _CACHED_RUNES - 1. 
	    The following are to deal with Runes larger than _CACHED_RUNES - 1.
//CMNT: possible_pointer ^|       	 * Their data is actually contiguous with this structure so as to make 
	    Their data is actually contiguous with this structure so as to make
//CMNT: possible_pointer ^|       	 * it easier to read/write from/to disk. 
	    it easier to read/write from/to disk.
	 */
 _RuneRange __runetype_ext;
 _RuneRange __maplower_ext;
 _RuneRange __mapupper_ext;

//CMNT: c_keyword possible_pointer ^|        void *__variable; /* Data which depends on the encoding */ 
  _ _variable; /* Data which depends on the encoding */
//CMNT: c_keyword ^|        int __variable_len; /* how long that data is */ 
  __variable_len; /* how long that data is */

 /*
//CMNT: c_keyword possible_pointer ^|       	 * extra fields to deal with arbitrary character classes 
	    extra fields to deal with arbitrary acter classes
	 */
//CMNT: c_keyword ^|        int __ncharclasses; 
  __nclasses;
//CMNT: c_keyword possible_pointer ^|        _RuneCharClass *__charclasses; 
 _RuneCharClass  _ _classes;
} _RuneLocale;



extern "C" {
extern _RuneLocale _DefaultRuneLocale;
//CMNT: possible_pointer ^|       extern _RuneLocale *_CurrentRuneLocale; 
extern _RuneLocale  _ CurrentRuneLocale;
}
