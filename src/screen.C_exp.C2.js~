 set (K  o bject){
    self = object;
    func = thunk<K, method>;
  }

//CMNT: c_keyword ^|         R call (A1 a1, A2 a2, A3 a3, A4 a4, A5 a5) const 
  R call (A1 a1, A2 a2, A3 a3, A4 a4, A5 a5) 
  {
    return func (self, a1, a2, a3, a4, a5);
  }

//CMNT: c_keyword ^|         R operator ()(A1 a1, A2 a2, A3 a3, A4 a4, A5 a5) const 
  R operator ()(A1 a1, A2 a2, A3 a3, A4 a4, A5 a5) 
  {
    return call (a1, a2, a3, a4, a5);
  }

