 tcsetattr(, ,  struct termios  ) ;
//CMNT: c_keyword remove_casts ^|       int tcdrain(int) __asm("_" "tcdrain" /* nothing */ "$UNIX2003"); 
 tcdrain __asm("_" "tcdrain" /* nothing */ "$UNIX2003");
//CMNT: c_keyword ^|       int tcflow(int, int); 
 tcflow(, );
//CMNT: c_keyword ^|       int tcflush(int, int); 
 tcflush(, );
//CMNT: c_keyword ^|       int tcsendbreak(int, int); 
 tcsendbreak(, );


//CMNT: c_keyword possible_pointer ^|       void cfmakeraw(struct termios *); 
 cfmakeraw(struct termios  ) ;
//CMNT: c_keyword possible_pointer ^|       int cfsetspeed(struct termios *, speed_t); 
 cfsetspeed(struct termios  ,  speed_t);

}




