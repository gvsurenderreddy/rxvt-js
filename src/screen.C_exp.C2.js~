  using ::wprf;
  using ::wscanf;

  using ::wcschr;

//CMNT: c_keyword ^|         inline wchar_t* 
  inline *
//CMNT: c_keyword possible_pointer ^|         wcschr(wchar_t* __p, wchar_t __c){ return wcschr(const_cast<const wchar_t*>(__p), __c); } 
  wcschr(   __p,  __c){ return wcschr(_cast<  > (__p), __c); }

  using ::wcspbrk;

//CMNT: c_keyword ^|         inline wchar_t* 
  inline *
//CMNT: c_keyword possible_pointer ^|         wcspbrk(wchar_t* __s1, wchar_t* __s2){ return wcspbrk(const_cast<const wchar_t*>(__s1), __s2); } 
  wcspbrk(   __s1,    __s2){ return wcspbrk(_cast<  > (__s1), __s2); }

  using ::wcsrchr;

//CMNT: c_keyword ^|         inline wchar_t* 
  inline *
//CMNT: c_keyword possible_pointer ^|         wcsrchr(wchar_t* __p, wchar_t __c){ return wcsrchr(const_cast<const wchar_t*>(__p), __c); } 
  wcsrchr(   __p,  __c){ return wcsrchr(_cast<  > (__p), __c); }

  using ::wcsstr;

//CMNT: c_keyword ^|         inline wchar_t* 
  inline *
//CMNT: c_keyword possible_pointer ^|         wcsstr(wchar_t* __s1, const wchar_t* __s2){ return wcsstr(const_cast<const wchar_t*>(__s1), __s2); } 
  wcsstr(   __s1,     __s2){ return wcsstr(_cast<  > (__s1), __s2); }

  using ::wmemchr;

//CMNT: c_keyword ^|         inline wchar_t* 
  inline *
//CMNT: c_keyword possible_pointer ^|         wmemchr(wchar_t* __p, wchar_t __c, size_t __n){ return wmemchr(const_cast<const wchar_t*>(__p), __c, __n); } 
  wmemchr(   __p,  __c, size_t __n){ return wmemchr(_cast<  > (__p), __c, __n); }
}







