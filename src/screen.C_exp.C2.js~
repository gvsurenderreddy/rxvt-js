namespace ev {

  typedef ev_tstamp tstamp;

  enum
  {
    UNDEF = -1 /* guaranteed to be invalid */,
    NONE = 0x00 /* no events */,
    READ = 0x01 /* ev_io detected read will not block */,
    WRITE = 0x02 /* ev_io detected write will not block */,
    TIMEOUT = 0x00000100 /* timer timed out */,
    PERIODIC = 0x00000200 /* periodic timer timed out */,
    SIGNAL = 0x00000400 /* signal was received */,
    CHILD = 0x00000800 /* child/pid had status change */,
    STAT = 0x00001000 /* stat data changed */,
    IDLE = 0x00002000 /* event loop is idling */,
    CHECK = 0x00008000 /* event loop finished poll */,
    PREPARE = 0x00004000 /* event loop about to poll */,
    FORK = 0x00020000 /* event loop resumed in child */,
//CMNT: c_keyword ^|           ASYNC = 0x00040000 /* async intra-loop signal */, 
    ASYNC = 0x00040000 /* async ra-loop signal */,
    EMBED = 0x00010000 /* embedded event loop needs sweep */,

    ERROR = 0x80000000 /* sent when an error occurs */,
  };

  enum
  {
    AUTO = 0x00000000U /* not quite a mask */,
    NOENV = 0x01000000U /* do NOT consult environment */,
    FORKCHECK = 0x02000000U /* check for a fork in each iteration */,

    SELECT = 0x00000001U /* about anywhere */,
    POLL = 0x00000002U /* !win */,
    EPOLL = 0x00000004U /* linux */,
    KQUEUE = 0x00000008U /* bsd */,
    DEVPOLL = 0x00000010U /* solaris 8 */ /* NYI */,
    PORT = 0x00000020U /* solaris 10 */
  };

  enum
  {
    NONBLOCK = 1 /* do not block/wait */,
//CMNT: possible_pointer ^|           ONESHOT = 2 /* block *once* only */ 
    ONESHOT = 2 /* block  o nce   only */
  };

  enum how_t
  {
    ONE = 1 /* unloop once */,
    ALL = 2 /* unloop all loops */
  };

  struct bad_loop



  {






  };
      //     # 137 "./../libev/ev++.h"
  struct loop_ref
  {
//CMNT: remove_casts ^|           loop_ref (void) throw () 
    loop_ref  throw ()



    {
    }

//CMNT: c_keyword ^|           bool operator == (const loop_ref &other) const throw (){ 
    bool operator == ( loop_ref &other)  throw (){



      return true;

    }

//CMNT: c_keyword ^|           bool operator != (const loop_ref &other) const throw (){ 
    bool operator != ( loop_ref &other)  throw (){



      return false;

    }
      //     # 191 "./../libev/ev++.h"
//CMNT: c_keyword ^|           void loop (int flags = 0){ 
 loop ( flags = 0){
      ev_loop ( flags);
    }

//CMNT: c_keyword ^|           void unloop (how_t how = ONE) throw (){ 
 unloop (how_t how = ONE) throw (){
      ev_unloop ( how);
    }

//CMNT: c_keyword ^|           void post_fork () throw (){ 
 post_fork () throw (){



      ev_default_fork ();

    }

//CMNT: c_keyword ^|           unsigned int count () const throw (){ 
      count ()  throw (){
      return ev_loop_count ();
    }

//CMNT: c_keyword ^|           unsigned int backend () const throw (){ 
      backend ()  throw (){
      return ev_backend ();
    }

//CMNT: c_keyword ^|           tstamp now () const throw (){ 
    tstamp now ()  throw (){
      return ev_now ();
    }

//CMNT: c_keyword ^|           void ref () throw (){ 
 ref () throw (){
      ev_ref ();
    }

//CMNT: c_keyword ^|           void unref () throw (){ 
 unref () throw (){
      ev_unref ();
    }

//CMNT: c_keyword ^|           void set_io_collect_interval (tstamp interval) throw (){ 
 set_io_collect_erval (tstamp erval) throw (){
//CMNT: c_keyword ^|             ev_set_io_collect_interval ( interval); 
      ev_set_io_collect_erval ( erval);
    }

//CMNT: c_keyword ^|           void set_timeout_collect_interval (tstamp interval) throw (){ 
 set_timeout_collect_erval (tstamp erval) throw (){
//CMNT: c_keyword ^|             ev_set_timeout_collect_interval ( interval); 
      ev_set_timeout_collect_erval ( erval);
    }

    // function callback
//CMNT: c_keyword possible_pointer ^|           void once (int fd, int events, tstamp timeout, void (*cb)(int, void *), void *arg = 0) throw (){ 
 once ( fd,  events, tstamp timeout, ( c b)(,  ) ,  a rg = 0) throw (){
      ev_once ( fd, events, timeout, cb, arg);
    }

    // method callback
//CMNT: c_keyword possible_pointer remove_casts ^|           template<class K, void (K::*method)(int)> 
    template<class K, (K:: m ethod)>
//CMNT: c_keyword possible_pointer ^|           void once (int fd, int events, tstamp timeout, K *object) throw (){ 
 once ( fd,  events, tstamp timeout, K  o bject) throw (){
      once (fd, events, timeout, method_thunk<K, method>, object);
    }

    // default method == operator ()
    template<class K>
//CMNT: c_keyword possible_pointer ^|           void once (int fd, int events, tstamp timeout, K *object) throw (){ 
 once ( fd,  events, tstamp timeout, K  o bject) throw (){
//CMNT: js_style_functions ^|             once (fd, events, timeout, method_thunk<K, &K::operator ()>, object); 
      once (fd, events, timeout, method_thunk<K, &K.operator =function()>, object); 
    }

//CMNT: c_keyword possible_pointer remove_casts ^|           template<class K, void (K::*method)(int)> 
    template<class K, (K:: m ethod)>
//CMNT: c_keyword possible_pointer ^|           static void method_thunk (int revents, void *arg){ 
     method_thunk ( revents,  a rg){
//CMNT: c_keyword possible_pointer ^|             static_cast<K *>(arg)->*method 
      _cast<K  > (arg)-> m ethod
        (revents);
    }

    // no-argument method callback
//CMNT: c_keyword possible_pointer ^|           template<class K, void (K::*method)()> 
    template<class K, (K:: m ethod)()>
//CMNT: c_keyword possible_pointer ^|           void once (int fd, int events, tstamp timeout, K *object) throw (){ 
 once ( fd,  events, tstamp timeout, K  o bject) throw (){
      once (fd, events, timeout, method_noargs_thunk<K, method>, object);
    }

//CMNT: c_keyword possible_pointer ^|           template<class K, void (K::*method)()> 
    template<class K, (K:: m ethod)()>
//CMNT: c_keyword possible_pointer ^|           static void method_noargs_thunk (int revents, void *arg){ 
     method_noargs_thunk ( revents,  a rg){
//CMNT: c_keyword possible_pointer ^|             static_cast<K *>(arg)->*method 
      _cast<K  > (arg)-> m ethod
        ();
    }

    // simpler function callback
//CMNT: c_keyword possible_pointer remove_casts ^|           template<void (*cb)(int)> 
    template< ( c b)>
//CMNT: c_keyword ^|           void once (int fd, int events, tstamp timeout) throw (){ 
 once ( fd,  events, tstamp timeout) throw (){
      once (fd, events, timeout, simpler_func_thunk<cb>);
    }

//CMNT: c_keyword possible_pointer remove_casts ^|           template<void (*cb)(int)> 
    template< ( c b)>
//CMNT: c_keyword possible_pointer ^|           static void simpler_func_thunk (int revents, void *arg){ 
     simpler_func_thunk ( revents,  a rg){
//CMNT: possible_pointer ^|             (*cb) 
      ( c b)
        (revents);
    }

    // simplest function callback
//CMNT: c_keyword possible_pointer ^|           template<void (*cb)()> 
    template< ( c b)()>
//CMNT: c_keyword ^|           void once (int fd, int events, tstamp timeout) throw (){ 
 once ( fd,  events, tstamp timeout) throw (){
      once (fd, events, timeout, simplest_func_thunk<cb>);
    }

//CMNT: c_keyword possible_pointer ^|           template<void (*cb)()> 
    template< ( c b)()>
//CMNT: c_keyword possible_pointer ^|           static void simplest_func_thunk (int revents, void *arg){ 
     simplest_func_thunk ( revents,  a rg){
//CMNT: possible_pointer ^|             (*cb) 
      ( c b)
        ();
    }

//CMNT: c_keyword ^|           void feed_fd_event (int fd, int revents) throw (){ 
 feed_fd_event ( fd,  revents) throw (){
      ev_feed_fd_event ( fd, revents);
    }

//CMNT: c_keyword ^|           void feed_signal_event (int signum) throw (){ 
 feed_signal_event ( signum) throw (){
      ev_feed_signal_event ( signum);
    }





  };
      //     # 356 "./../libev/ev++.h"
  struct default_loop : loop_ref
  {
//CMNT: c_keyword ^|           default_loop (unsigned int flags = AUTO) throw (bad_loop) 
    default_loop (  flags = AUTO) throw (bad_loop)



    {
      if (



          !ev_default_loop (flags)

      )
        throw bad_loop ();
    }

    ~default_loop () throw (){
      ev_default_destroy ();
    }

  private:
//CMNT: c_keyword ^|           default_loop (const default_loop &); 
    default_loop ( default_loop &);
//CMNT: c_keyword ^|           default_loop &operator = (const default_loop &); 
    default_loop &operator = ( default_loop &);
  };

  inline loop_ref get_default_loop () throw (){



    return loop_ref ();

  }
      //     # 405 "./../libev/ev++.h"
  template<class ev_watcher, class watcher>
  struct base : ev_watcher
  {
      //     # 417 "./../libev/ev++.h"
    base () throw ()



    {
//CMNT: c_keyword possible_pointer ^|             do { ((ev_watcher *)(void *)(this))->active = ((ev_watcher *)(void *)(this))->pending = ((ev_watcher *)(void *)(this))->priority = 0; ((this))->cb /* rw */ = (0); } while (0); 
      do { ((ev_watcher  ) (  ) (this))->active = ((ev_watcher  ) (  ) (this))->pending = ((ev_watcher  ) (  ) (this))->priority = 0; ((this))->cb /* rw */ = (0); } while (0);
    }

//CMNT: c_keyword possible_pointer ^|           void set_ (const void *data, void (*cb)( ev_watcher *w, int revents)) throw (){ 
 set_ (  d ata, ( c b)( ev_watcher  w ,  revents)) throw (){
//CMNT: c_keyword possible_pointer ^|             this->data = (void *)data; 
      this->data = (  ) data;
//CMNT: c_keyword possible_pointer ^|             (static_cast<ev_watcher *>(this))->cb /* rw */ = (cb); 
      (_cast<ev_watcher  > (this))->cb /* rw */ = (cb);
    }

    // function callback
//CMNT: c_keyword possible_pointer ^|           template<void (*function)(watcher &w, int)> 
    template< ( f unction)(watcher &w, )>
//CMNT: c_keyword possible_pointer ^|           void set (void *data = 0) throw (){ 
