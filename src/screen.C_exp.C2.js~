 __srget(FILE  ) ;
//CMNT: c_keyword possible_pointer ^|       int __svfscanf(FILE *, const char *, va_list) /* nothing */; 
 __svfscanf(FILE  ,     ,  va_list) /* nothing */;
//CMNT: c_keyword possible_pointer ^|       int __swbuf(int, FILE *); 
 __swbuf(, FILE  ) ;
}

/*
//CMNT: possible_pointer ^|        * The __sfoo macros are here so that we can 
    The __sfoo macros are here so that we can
//CMNT: possible_pointer ^|        * define function versions in the C library. 
    define function versions in the C library.
 */


//CMNT: c_keyword possible_pointer ^|       static inline /* convert to C++ keyword */ int __sputc(int _c, FILE *_p) { 
 inline /* convert to C++ keyword */  __sputc( _c, FILE  _ p) {
//CMNT: remove_casts ^|        if (--_p->_w >= 0 || (_p->_w >= _p->_lbfsize && (char)_c != '\n')) 
 if (--_p->_w >= 0 || (_p->_w >= _p->_lbfsize && _c != '\n'))
//CMNT: possible_pointer ^|         return (*_p->_p++ = _c); 
  return ( _ p->_p++ = _c);
 else
  return (__swbuf(_c, _p));
}
