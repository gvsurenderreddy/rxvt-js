  s trstr(   ,     ) ;
//CMNT: c_keyword possible_pointer ^|       char *strtok(char *, const char *); 
  s trtok(  ,     ) ;
//CMNT: c_keyword possible_pointer ^|       size_t strxfrm(char *, const char *, size_t); 
size_t strxfrm(  ,     ,  size_t);

