//CMNT: c_keyword ^|           void operator ()(int events = -1 /* guaranteed to be invalid */){ 
 operator ()( events = -1 /* guaranteed to be invalid */){
      return
//CMNT: c_keyword possible_pointer ^|               (static_cast<ev_watcher *>(this))->cb /* rw */ 
        (_cast<ev_watcher  > (this))->cb /* rw */
//CMNT: c_keyword possible_pointer ^|                 (static_cast<ev_watcher *>(this), events); 
          (_cast<ev_watcher  > (this), events);
