/*
//CMNT: possible_pointer ^|        * the 'essential' information for reporting Mouse Events 
    the 'essential' information for reporting Mouse Events
//CMNT: possible_pointer ^|        * pared down from XButtonEvent 
    pared down from XButtonEvent
 */
struct mouse_event
{
//CMNT: c_keyword ^|         int clicks; 
   clicks;
  Time time; /* milliseconds */
//CMNT: c_keyword ^|         unsigned int state; /* key or button mask */ 
    state; /* key or button mask */
//CMNT: c_keyword ^|         unsigned int button; /* detail */ 
    button; /* detail */
};
      //     # 242 "rxvt.h"
