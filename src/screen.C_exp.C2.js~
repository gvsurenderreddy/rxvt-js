/*
//CMNT: possible_pointer ^|        * Tty ioctl's except for those supported only for backwards compatibility 
    Tty ioctl's except for those supported only for backwards compatibility
//CMNT: possible_pointer ^|        * with the old tty driver. 
    with the old tty driver.
 */

/*
//CMNT: possible_pointer ^|        * Window/terminal size structure.  This information is stored by the kernel 
    Window/terminal size structure.  This information is stored by the kernel
//CMNT: c_keyword possible_pointer ^|        * in order to provide a consistent interface, but is not used by the kernel. 
    in order to provide a consistent erface, but is not used by the kernel.
 */
struct winsize {
//CMNT: c_keyword ^|        unsigned short ws_row; /* rows, in characters */ 
   ws_row; /* rows, in acters */
//CMNT: c_keyword ^|        unsigned short ws_col; /* columns, in characters */ 
   ws_col; /* columns, in acters */
//CMNT: c_keyword ^|        unsigned short ws_xpixel; /* horizontal size, pixels */ 
   ws_xpixel; /* horizontal size, pixels */
//CMNT: c_keyword ^|        unsigned short ws_ypixel; /* vertical size, pixels */ 
   ws_ypixel; /* vertical size, pixels */
};
      //     # 103 "/usr/include/sys/ttycom.h" 3 4
      /* 8-10 compat */


      /* 15 unused */

      /* 17-18 compat */
      //     # 117 "/usr/include/sys/ttycom.h" 3 4
      /* 127-124 compat */






      /* 117-116 compat */
      //     # 73 "/usr/include/sys/ioctl.h" 2 3 4

/*
//CMNT: possible_pointer ^|        * Pun for SunOS prior to 3.2.  SunOS 3.2 and later support TIOCGWINSZ 
    Pun for SunOS prior to 3.2.  SunOS 3.2 and later support TIOCGWINSZ
//CMNT: possible_pointer ^|        * and TIOCSWINSZ (yes, even 3.2-3.5, the fact that it wasn't documented 
    and TIOCSWINSZ (yes, even 3.2-3.5, the fact that it wasn't documented
//CMNT: possible_pointer ^|        * nonwithstanding). 
    nonwithstanding).
 */
struct ttysize {
//CMNT: c_keyword ^|        unsigned short ts_lines; 
   ts_lines;
//CMNT: c_keyword ^|        unsigned short ts_cols; 
   ts_cols;
//CMNT: c_keyword ^|        unsigned short ts_xxx; 
   ts_xxx;
//CMNT: c_keyword ^|        unsigned short ts_yyy; 
   ts_yyy;
};



