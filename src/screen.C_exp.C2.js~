  x, y; /* poer x, y coordinates in event window */
//CMNT: c_keyword ^|        int x_root, y_root; /* coordinates relative to root */ 
  x_root, y_root; /* coordinates relative to root */
//CMNT: c_keyword ^|        unsigned int state; /* key or button mask */ 
   state; /* key or button mask */
//CMNT: c_keyword ^|        unsigned int button; /* detail */ 
   button; /* detail */
//CMNT: c_keyword ^|        int same_screen; /* same screen flag */ 
  same_screen; /* same screen flag */
} XButtonEvent;
typedef XButtonEvent XButtonPressedEvent;
typedef XButtonEvent XButtonReleasedEvent;

typedef struct {
//CMNT: c_keyword ^|        int type; /* of event */ 
  type; /* of event */
//CMNT: c_keyword ^|        unsigned long serial; /* # of last request processed by server */ 
  long serial; /* # of last request processed by server */
//CMNT: c_keyword ^|        int send_event; /* true if this came from a SendEvent request */ 
  send_event; /* true if this came from a SendEvent request */
//CMNT: possible_pointer ^|        Display *display; /* Display the event was read from */ 
 Display  d isplay; /* Display the event was read from */
 Window window; /* "event" window reported relative to */
 Window root; /* root window that the event occurred on */
 Window subwindow; /* child window */
 Time time; /* milliseconds */
//CMNT: c_keyword ^|        int x, y; /* pointer x, y coordinates in event window */ 
  x, y; /* poer x, y coordinates in event window */
//CMNT: c_keyword ^|        int x_root, y_root; /* coordinates relative to root */ 
  x_root, y_root; /* coordinates relative to root */
//CMNT: c_keyword ^|        unsigned int state; /* key or button mask */ 
   state; /* key or button mask */
