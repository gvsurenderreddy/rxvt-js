/*
//CMNT: c_keyword possible_pointer ^|        * Basic integral types.  Omit the typedef if 
    Basic egral types.  Omit the typedef if
//CMNT: possible_pointer ^|        * not possible for a machine/compiler combination. 
    not possible for a machine/compiler combination.
 */


//CMNT: c_keyword ^|       typedef signed char int8_t; 
typedef signed  8_t;

//CMNT: c_keyword ^|       typedef unsigned char u_int8_t; 
typedef   u_8_t;


//CMNT: c_keyword ^|       typedef short int16_t; 
typedef  16_t;

//CMNT: c_keyword ^|       typedef unsigned short u_int16_t; 
typedef   u_16_t;


//CMNT: c_keyword ^|       typedef int int32_t; 
typedef  32_t;

//CMNT: c_keyword ^|       typedef unsigned int u_int32_t; 
typedef   u_32_t;


//CMNT: c_keyword ^|       typedef long long int64_t; 
typedef long long 64_t;

//CMNT: c_keyword ^|       typedef unsigned long long u_int64_t; 
typedef  long long u_64_t;




//CMNT: c_keyword ^|       typedef int32_t register_t; 
typedef 32_t register_t;




//CMNT: c_keyword ^|       typedef __darwin_intptr_t intptr_t; 
typedef __darwin_ptr_t ptr_t;



//CMNT: c_keyword ^|       typedef unsigned long int uintptr_t; 
typedef  long  uptr_t;



/* These types are used for reserving the largest possible size. */
//CMNT: c_keyword ^|       typedef u_int64_t user_addr_t; 
typedef u_64_t user_addr_t;
//CMNT: c_keyword ^|       typedef u_int64_t user_size_t; 
typedef u_64_t user_size_t;
//CMNT: c_keyword ^|       typedef int64_t user_ssize_t; 
typedef 64_t user_ssize_t;
//CMNT: c_keyword ^|       typedef int64_t user_long_t; 
typedef 64_t user_long_t;
//CMNT: c_keyword ^|       typedef u_int64_t user_ulong_t; 
typedef u_64_t user_ulong_t;
//CMNT: c_keyword ^|       typedef int64_t user_time_t; 
typedef 64_t user_time_t;




//CMNT: c_keyword ^|       /* This defines the size of syscall arguments after copying into the kernel: */ 
/* This defines the size of syscall arguments after copying o the kernel: */
//CMNT: c_keyword ^|       typedef u_int64_t syscall_arg_t; 
typedef u_64_t syscall_arg_t;
