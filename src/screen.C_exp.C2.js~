typedef  long Pixel;

struct rgba
{
//CMNT: c_keyword ^|         unsigned short r, g, b, a; 
    r, g, b, a;

  enum { MIN_CC = 0x0000, MAX_CC = 0xffff };

  rgba (){ }

//CMNT: c_keyword ^|         rgba (unsigned short r, unsigned short g, unsigned short b, unsigned short a = MAX_CC) 
  rgba (  r,   g,   b,   a = MAX_CC)
  : r(r), g(g), b(b), a(a){ }
};

struct rxvt_color
{



  XColor c;


//CMNT: c_keyword ^|         operator Pixel () const { return c.pixel; } 
  operator Pixel ()  { return c.pixel; }

//CMNT: c_keyword possible_pointer ^|         bool operator == (const rxvt_color &b) const { return Pixel (*this) == Pixel (b); } 
  bool operator == ( rxvt_color &b)  { return Pixel ( t his) == Pixel (b); }
//CMNT: c_keyword possible_pointer ^|         bool operator != (const rxvt_color &b) const { return Pixel (*this) != Pixel (b); } 
  bool operator != ( rxvt_color &b)  { return Pixel ( t his) != Pixel (b); }

//CMNT: c_keyword ^|         bool is_opaque () const 
  bool is_opaque () 
  {



    return 1;

  }

//CMNT: c_keyword possible_pointer ^|         bool alloc (rxvt_screen *screen, const rgba &color); 
  bool alloc (rxvt_screen  s creen,  rgba &color);
//CMNT: c_keyword possible_pointer ^|         void free (rxvt_screen *screen); 
 free (rxvt_screen  s creen);

//CMNT: c_keyword ^|         void get (rgba &color); 
 get (rgba &color);
//CMNT: c_keyword ^|         void get (XColor &color); 
 get (XColor &color);

//CMNT: c_keyword possible_pointer ^|         bool set (rxvt_screen *screen, const char *name); 
  bool set (rxvt_screen  s creen,    n ame);
//CMNT: c_keyword possible_pointer ^|         bool set (rxvt_screen *screen, const rgba &color); 
  bool set (rxvt_screen  s creen,  rgba &color);

//CMNT: c_keyword possible_pointer ^|         void fade (rxvt_screen *screen, int percent, rxvt_color &result, const rgba &to = rgba (0, 0, 0)); 
 fade (rxvt_screen  s creen,  percent, rxvt_color &result,  rgba &to = rgba (0, 0, 0));
};
      //     # 15 "rxvtfont.h" 2

struct rxvt_term;

struct rxvt_fontprop
{
  enum {
    unset = -1,
    medium = 100, bold = 200,
    roman = 0, italic = 100,
  };
//CMNT: c_keyword ^|         int width, height, ascent; 
   width, height, ascent;
//CMNT: c_keyword ^|         int weight, slant; 
   weight, slant;
};

struct rxvt_font
{
  // managed by the fontset
//CMNT: possible_pointer ^|         rxvt_term *term; 
  rxvt_term  t erm;
//CMNT: c_keyword possible_pointer ^|         void set_term (rxvt_term *term) { this->term = term; } 
 set_term (rxvt_term  t erm) { this->term = term; }

//CMNT: c_keyword possible_pointer ^|         char *name; 
    n ame;
  codeset cs;
  bool loaded; // wether we tried loading it before (not wether it's loaded)

  // managed by the font object
//CMNT: c_keyword ^|         int ascent, descent, 
   ascent, descent,
      width, height;

//CMNT: c_keyword possible_pointer ^|         void set_name (char *name); 
 set_name (  n ame);

  rxvt_font ();
  virtual ~rxvt_font () { free (name); };

//CMNT: c_keyword ^|         virtual void clear () { }; 
  virtual clear () { };

//CMNT: c_keyword ^|         void clear_rect (rxvt_drawable &d, int x, int y, int w, int h, int color) const; 
 clear_rect (rxvt_drawable &d,  x,  y,  w,  h,  color) ;

  virtual rxvt_fontprop properties () = 0;

//CMNT: c_keyword ^|         virtual bool load (const rxvt_fontprop &morph, bool force_prop) = 0; 
  virtual bool load ( rxvt_fontprop &morph, bool force_prop) = 0;
//CMNT: c_keyword possible_pointer ^|         virtual bool has_char (uint32_t unicode, const rxvt_fontprop *prop, bool &careful) const = 0; 
  virtual bool has_ (u32_t unicode,  rxvt_fontprop  p rop, bool &careful)  = 0;

//CMNT: c_keyword ^|         virtual void draw (rxvt_drawable &d, 
  virtual draw (rxvt_drawable &d,
//CMNT: c_keyword ^|                            int x, int y, 
                      x,  y,
//CMNT: c_keyword possible_pointer ^|                            const text_t *text, int len, 
                      text_t  t ext,  len,
//CMNT: c_keyword ^|                            int fg, int bg) = 0; 
                      fg,  bg) = 0;
};



struct rxvt_fallback_font;

struct rxvt_fontset
{
//CMNT: c_keyword possible_pointer ^|         char *fontdesc; 
    f ontdesc;

//CMNT: possible_pointer ^|         rxvt_fontset (rxvt_term *term); 
  rxvt_fontset (rxvt_term  t erm);
  ~rxvt_fontset ();

//CMNT: c_keyword possible_pointer ^|         bool populate (const char *desc); 
  bool populate (   d esc);
//CMNT: c_keyword ^|         void set_prop (const rxvt_fontprop &prop, bool force_prop) { this->prop = prop; this->force_prop = force_prop; } 
 set_prop ( rxvt_fontprop &prop, bool force_prop) { this->prop = prop; this->force_prop = force_prop; }
//CMNT: c_keyword ^|         int find_font (uint32_t unicode); 
   find_font (u32_t unicode);
//CMNT: c_keyword possible_pointer ^|         int find_font (const char *name) const; 
   find_font (   n ame) ;
//CMNT: c_keyword ^|         bool realize_font (int i); 
  bool realize_font ( i);

//CMNT: c_keyword ^|         // font-id's MUST fit into a signed 16 bit integer, and within 0..255 
  // font-id's MUST fit o a signed 16 bit eger, and within 0..255
//CMNT: c_keyword possible_pointer ^|         rxvt_font *operator [] (int id) const 
  rxvt_font  o perator [] ( id) 
  {
    return fonts[id & 0x7f];
  }

private:
//CMNT: possible_pointer ^|         rxvt_term *term; 
  rxvt_term  t erm;
  rxvt_fontprop prop;
  bool force_prop;
//CMNT: possible_pointer ^|         simplevec<rxvt_font *> fonts; 
  simplevec<rxvt_font  >  fonts;
//CMNT: c_keyword possible_pointer ^|         const rxvt_fallback_font *fallback; 
   rxvt_fallback_font  f allback;

//CMNT: c_keyword ^|         typedef unsigned char pagemap[256]; 
  typedef   pagemap[256];
//CMNT: possible_pointer ^|         vector<pagemap *> fmap; 
  vector<pagemap  >  fmap;

//CMNT: c_keyword ^|         void clear (); 
 clear ();
//CMNT: c_keyword possible_pointer ^|         rxvt_font *new_font (const char *name, codeset cs); 
  rxvt_font  n ew_font (   n ame, codeset cs);
//CMNT: c_keyword possible_pointer ^|         void add_fonts (const char *desc); 
 add_fonts (   d esc);
};
