  _flags; /* flags, below; this FILE is free if 0 */
//CMNT: c_keyword ^|        short _file; /* fileno, if Unix descriptor, else -1 */ 
  _file; /* fileno, if Unix descriptor, else -1 */
 struct __sbuf _bf; /* the buffer (at least 1 byte, if !NULL) */
//CMNT: c_keyword ^|        int _lbfsize; /* 0 or -_bf._size, for inline putc */ 
  _lbfsize; /* 0 or -_bf._size, for inline putc */

