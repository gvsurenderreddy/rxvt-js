      //     # 270 "rxvttoolkit.h"
};
      //     # 287 "rxvttoolkit.h"
//CMNT: c_keyword ^|       struct xevent_watcher : rxvt_watcher, callback<void (XEvent &)> 
struct xevent_watcher : rxvt_watcher, callback< (XEvent &)>
{
  Window window;

//CMNT: c_keyword possible_pointer ^|         void start (rxvt_display *display, Window window){ 
 start (rxvt_display  d isplay, Window window){
    this->window = window;
    display->reg (this);
  }

//CMNT: c_keyword possible_pointer ^|         void stop (rxvt_display *display){ 
 stop (rxvt_display  d isplay){
    display->unreg (this);
  }
};

extern refcache<rxvt_display> displays;

