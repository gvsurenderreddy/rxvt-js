   R thunk (  s elf, A1 a1, A2 a2, A3 a3, A4 a4){
//CMNT: c_keyword possible_pointer ^|           klass *obj = static_cast<klass *>(self); 
    klass  o bj = _cast<klass  > (self);
//CMNT: possible_pointer ^|           return (obj->*method) (a1, a2, a3, a4); 
    return (obj-> m ethod) (a1, a2, a3, a4);
  }
};

template<class R, class A1, class A2, class A3, class A4, class A5>
struct callback<R (A1, A2, A3, A4, A5)>
{
//CMNT: c_keyword possible_pointer ^|         typedef R (*ptr_type)(void *self, A1, A2, A3, A4, A5); 
  typedef R ( p tr_type)(  s elf, A1, A2, A3, A4, A5);

//CMNT: possible_pointer ^|         template<class K, R (K::*method)(A1, A2, A3, A4, A5)> 
  template<class K, R (K:: m ethod)(A1, A2, A3, A4, A5)>
