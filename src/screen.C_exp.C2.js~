//CMNT: c_keyword possible_pointer ^|           template<class K, void (K::*method)()> 
    template<class K, (K:: m ethod)()>
//CMNT: c_keyword possible_pointer ^|           void set (K *object) throw (){ 
 set (K  o bject) throw (){
      set_ (object, method_noargs_thunk<K, method>);
