/*
//CMNT: possible_pointer ^|        * i386 is the structure that is exported to user threads for  
    i386 is the structure that is exported to user threads for 
//CMNT: possible_pointer ^|        * use in status/mutate calls.  This structure should never change. 
    use in status/mutate calls.  This structure should never change.
 *
 */



struct __darwin_i386_thread_state
{
//CMNT: c_keyword ^|           unsigned int __eax; 
      __eax;
//CMNT: c_keyword ^|           unsigned int __ebx; 
      __ebx;
//CMNT: c_keyword ^|           unsigned int __ecx; 
      __ecx;
//CMNT: c_keyword ^|           unsigned int __edx; 
      __edx;
//CMNT: c_keyword ^|           unsigned int __edi; 
      __edi;
//CMNT: c_keyword ^|           unsigned int __esi; 
      __esi;
//CMNT: c_keyword ^|           unsigned int __ebp; 
      __ebp;
//CMNT: c_keyword ^|           unsigned int __esp; 
      __esp;
//CMNT: c_keyword ^|           unsigned int __ss; 
      __ss;
//CMNT: c_keyword ^|           unsigned int __eflags; 
      __eflags;
//CMNT: c_keyword ^|           unsigned int __eip; 
      __eip;
//CMNT: c_keyword ^|           unsigned int __cs; 
      __cs;
//CMNT: c_keyword ^|           unsigned int __ds; 
      __ds;
//CMNT: c_keyword ^|           unsigned int __es; 
      __es;
//CMNT: c_keyword ^|           unsigned int __fs; 
      __fs;
//CMNT: c_keyword ^|           unsigned int __gs; 
      __gs;
};
      //     # 85 "/usr/include/mach/i386/_structs.h" 3 4
/* This structure should be double-word aligned for performance */



struct __darwin_fp_control
{
//CMNT: c_keyword ^|           unsigned short __invalid :1, 
      __invalid :1,
        __denorm :1,
    __zdiv :1,
    __ovrfl :1,
    __undfl :1,
    __precis :1,
      :2,
    __pc :2,





    __rc :2,






     /*inf*/ :1,
      :3;
};
typedef struct __darwin_fp_control __darwin_fp_control_t;
      //     # 141 "/usr/include/mach/i386/_structs.h" 3 4
/*
//CMNT: possible_pointer ^|        * Status word. 
    Status word.
 */



struct __darwin_fp_status
{
//CMNT: c_keyword ^|           unsigned short __invalid :1, 
      __invalid :1,
        __denorm :1,
    __zdiv :1,
    __ovrfl :1,
    __undfl :1,
    __precis :1,
    __stkflt :1,
    __errsumm :1,
    __c0 :1,
    __c1 :1,
    __c2 :1,
    __tos :3,
    __c3 :1,
    __busy :1;
};
typedef struct __darwin_fp_status __darwin_fp_status_t;
      //     # 187 "/usr/include/mach/i386/_structs.h" 3 4
/* defn of 80bit x87 FPU or MMX register  */



struct __darwin_mmst_reg
{
//CMNT: c_keyword ^|        char __mmst_reg[10]; 
  __mmst_reg[10];
//CMNT: c_keyword ^|        char __mmst_rsrv[6]; 
  __mmst_rsrv[6];
};
      //     # 206 "/usr/include/mach/i386/_structs.h" 3 4
/* defn of 128 bit XMM regs */



struct __darwin_xmm_reg
{
//CMNT: c_keyword ^|        char __xmm_reg[16]; 
  __xmm_reg[16];
};
      //     # 222 "/usr/include/mach/i386/_structs.h" 3 4
/* 
//CMNT: c_keyword possible_pointer ^|        * Floating point state. 
    Floating po state.
 */







struct __darwin_i386_float_state
{
//CMNT: c_keyword ^|        int __fpu_reserved[2]; 
  __fpu_reserved[2];
 struct __darwin_fp_control __fpu_fcw; /* x87 FPU control word */
 struct __darwin_fp_status __fpu_fsw; /* x87 FPU status word */
//CMNT: c_keyword ^|        __uint8_t __fpu_ftw; /* x87 FPU tag word */ 
 __u8_t __fpu_ftw; /* x87 FPU tag word */
//CMNT: c_keyword ^|        __uint8_t __fpu_rsrv1; /* reserved */ 
 __u8_t __fpu_rsrv1; /* reserved */
//CMNT: c_keyword ^|        __uint16_t __fpu_fop; /* x87 FPU Opcode */ 
 __u16_t __fpu_fop; /* x87 FPU Opcode */
//CMNT: c_keyword ^|        __uint32_t __fpu_ip; /* x87 FPU Instruction Pointer offset */ 
 __u32_t __fpu_ip; /* x87 FPU Instruction Poer offset */
//CMNT: c_keyword ^|        __uint16_t __fpu_cs; /* x87 FPU Instruction Pointer Selector */ 
 __u16_t __fpu_cs; /* x87 FPU Instruction Poer Selector */
//CMNT: c_keyword ^|        __uint16_t __fpu_rsrv2; /* reserved */ 
 __u16_t __fpu_rsrv2; /* reserved */
//CMNT: c_keyword ^|        __uint32_t __fpu_dp; /* x87 FPU Instruction Operand(Data) Pointer offset */ 
 __u32_t __fpu_dp; /* x87 FPU Instruction Operand(Data) Poer offset */
//CMNT: c_keyword ^|        __uint16_t __fpu_ds; /* x87 FPU Instruction Operand(Data) Pointer Selector */ 
 __u16_t __fpu_ds; /* x87 FPU Instruction Operand(Data) Poer Selector */
//CMNT: c_keyword ^|        __uint16_t __fpu_rsrv3; /* reserved */ 
 __u16_t __fpu_rsrv3; /* reserved */
//CMNT: c_keyword ^|        __uint32_t __fpu_mxcsr; /* MXCSR Register state */ 
 __u32_t __fpu_mxcsr; /* MXCSR Register state */
//CMNT: c_keyword ^|        __uint32_t __fpu_mxcsrmask; /* MXCSR mask */ 
 __u32_t __fpu_mxcsrmask; /* MXCSR mask */
 struct __darwin_mmst_reg __fpu_stmm0; /* ST0/MM0   */
 struct __darwin_mmst_reg __fpu_stmm1; /* ST1/MM1  */
 struct __darwin_mmst_reg __fpu_stmm2; /* ST2/MM2  */
 struct __darwin_mmst_reg __fpu_stmm3; /* ST3/MM3  */
 struct __darwin_mmst_reg __fpu_stmm4; /* ST4/MM4  */
 struct __darwin_mmst_reg __fpu_stmm5; /* ST5/MM5  */
 struct __darwin_mmst_reg __fpu_stmm6; /* ST6/MM6  */
 struct __darwin_mmst_reg __fpu_stmm7; /* ST7/MM7  */
 struct __darwin_xmm_reg __fpu_xmm0; /* XMM 0  */
 struct __darwin_xmm_reg __fpu_xmm1; /* XMM 1  */
 struct __darwin_xmm_reg __fpu_xmm2; /* XMM 2  */
 struct __darwin_xmm_reg __fpu_xmm3; /* XMM 3  */
 struct __darwin_xmm_reg __fpu_xmm4; /* XMM 4  */
 struct __darwin_xmm_reg __fpu_xmm5; /* XMM 5  */
 struct __darwin_xmm_reg __fpu_xmm6; /* XMM 6  */
 struct __darwin_xmm_reg __fpu_xmm7; /* XMM 7  */
//CMNT: c_keyword possible_pointer ^|        char __fpu_rsrv4[14*16]; /* reserved */ 
  __fpu_rsrv4[14 1 6]; /* reserved */
//CMNT: c_keyword ^|        int __fpu_reserved1; 
  __fpu_reserved1;
};
