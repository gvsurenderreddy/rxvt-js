      //     # 5 "salloc.h" 2

// small blocks allocator

struct rxvt_salloc
{
  struct chain {
//CMNT: possible_pointer ^|           struct chain *next; 
    struct chain  n ext;
  };

//CMNT: possible_pointer ^|         chain *firstblock; 
  chain  f irstblock;
//CMNT: possible_pointer ^|         chain *firstline; 
  chain  f irstline;
//CMNT: c_keyword ^|         unsigned int firstfree; 
    firstfree;
//CMNT: c_keyword ^|         unsigned int size; 
    size;

//CMNT: c_keyword ^|         rxvt_salloc (unsigned int size); 
  rxvt_salloc (  size);
  ~rxvt_salloc ();

//CMNT: c_keyword possible_pointer ^|         void *alloc (); 
  a lloc ();
//CMNT: c_keyword possible_pointer ^|         void *alloc (void *data, unsigned int datalen); 
  a lloc (  d ata,   datalen);
//CMNT: c_keyword possible_pointer ^|         void free (void *data); 
 free (  d ata);
};
      //     # 84 "rxvt.h" 2
      //     # 1 "libptytty.h" 1
// This file is part of libptytty. Do not make local modifications.
// http://software.schmorp.de/pkg/libptytty






// C++ API

struct ptytty {
//CMNT: c_keyword ^|         int pty; // pty file descriptor; connected to terminal emulator 
   pty; // pty file descriptor; connected to terminal emulator
//CMNT: c_keyword ^|         int tty; // tty file descriptor; connected to child 
   tty; // tty file descriptor; connected to child

  virtual ~ptytty (){
  }

  virtual bool get () = 0;
//CMNT: c_keyword possible_pointer ^|         virtual void login (int cmd_pid, bool login_shell, const char *hostname) = 0; 
  virtual login ( cmd_pid, bool login_shell,    h ostname) = 0;

//CMNT: c_keyword ^|         void close_tty (); 
 close_tty ();
  bool make_controlling_tty ();
//CMNT: c_keyword ^|         void set_utf8_mode (bool on); 
 set_utf8_mode (bool on);

//CMNT: c_keyword ^|         static void sanitise_stdfd (); 
   sanitise_stdfd ();
//CMNT: c_keyword ^|         static void init (); 
   init ();
//CMNT: c_keyword possible_pointer ^|         static ptytty *create (); // create a new pty object 
   ptytty  c reate (); // create a new pty object

//CMNT: c_keyword ^|         static void drop_privileges (); 
   drop_privileges ();
//CMNT: c_keyword ^|         static void use_helper (); 
   use_helper ();

//CMNT: c_keyword ^|         static bool send_fd (int socket, int fd); 
   bool send_fd ( socket,  fd);
//CMNT: c_keyword ^|         static int recv_fd (int socket); 
    recv_fd ( socket);

protected:

  ptytty ()
  : pty(-1), tty(-1){
  }
};
