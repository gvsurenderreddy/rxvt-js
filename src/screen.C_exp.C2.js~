//CMNT: c_keyword possible_pointer ^|       int vsscanf(const char * , const char * , va_list) /* nothing */; 
 vsscanf(     ,      , va_list) /* nothing */;

//CMNT: c_keyword possible_pointer ^|       FILE *zopen(const char *, const char *, int); 
FILE  z open(   ,     ,  );

}

/*
//CMNT: c_keyword possible_pointer ^|        * Stdio function-access interface. 
    Stdio function-access erface.
 */

extern "C" {
//CMNT: c_keyword possible_pointer ^|       FILE *funopen(const void *, 
FILE  f unopen(  , 
//CMNT: c_keyword possible_pointer ^|         int (*)(void *, char *, int), 
   ( ) (  ,    ,  ),
//CMNT: c_keyword possible_pointer ^|         int (*)(void *, const char *, int), 
   ( ) (  ,     ,  ),
//CMNT: c_keyword possible_pointer ^|         fpos_t (*)(void *, fpos_t, int), 
  fpos_t ( ) (  ,  fpos_t, ),
//CMNT: c_keyword possible_pointer ^|         int (*)(void *)); 
   ( ) (  ) );
}





