  * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * 
 */


//CMNT: c_keyword ^|       // for speed reasons, we assume that all codepoints 32 to 126 are 
// for speed reasons, we assume that all codepos 32 to 126 are
// single-width.


//CMNT: c_keyword ^|       /* convert pixel dimensions to row/column values.  Everything as int32_t */ 
/* convert pixel dimensions to row/column values.  Everything as 32_t */
      //     # 626 "rxvt.h"
/* how to build & extract colors and attributes */
      //     # 658 "rxvt.h"
//CMNT: c_keyword possible_pointer ^|       typedef callback<void (const char *)> log_callback; 
typedef callback< (   ) > log_callback;
//CMNT: c_keyword remove_casts ^|       typedef callback<int (int)> getfd_callback; 
typedef callback< > getfd_callback;

//CMNT: possible_pointer ^|       /****************************************************************************/ 
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */







struct line_t
{
//CMNT: possible_pointer ^|          text_t *t; // terminal the text 
   text_t  t ; // terminal the text
//CMNT: c_keyword possible_pointer ^|          rend_t *r; // rendition, uses RS_ flags 
     r ; // rendition, uses RS_ flags
   tlen_t_ l; // length of each text line
//CMNT: c_keyword ^|          uint32_t f; // flags 
   u32_t f; // flags

   bool is_longer (){
     return f & 0x0001 /* line is continued on the next row*/;
   }

//CMNT: c_keyword ^|          void is_longer (int set){ 
 is_longer ( set){
     if (set)
       f |= 0x0001 /* line is continued on the next row*/;
     else
       f &= ~0x0001 /* line is continued on the next row*/;
   }

//CMNT: c_keyword ^|          void clear (){ 
 clear (){
     t = 0;
     r = 0;
     l = 0;
     f = 0;
   }

//CMNT: c_keyword ^|          void touch () // call whenever a line is changed/touched/updated 
 touch () // call whenever a line is changed/touched/updated
   {



   }

//CMNT: c_keyword ^|          void touch (int col){ 
 touch ( col){
     max_it (l, col);
     touch ();
   }
};

//CMNT: possible_pointer ^|       /****************************************************************************/ 
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */

// primitive wrapper around mbstate_t to ensure initialisation
struct mbstate
{
  mbstate_t mbs;

//CMNT: possible_pointer ^|         operator mbstate_t *() { return &mbs; } 
  operator mbstate_t  ( ) { return &mbs; }
//CMNT: c_keyword ^|         void reset () { (memset)(&mbs,0,sizeof (mbs)); } 
 reset () { (memset)(&mbs,0,sizeof (mbs)); }
  mbstate () { reset (); }
};

//CMNT: possible_pointer ^|       /****************************************************************************/ 
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */
      //     # 771 "rxvt.h"
//CMNT: possible_pointer ^|       /****************************************************************************/ 
/* * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * */





//CMNT: possible_pointer ^|       typedef struct rxvt_term *rxvt_t; 
typedef struct rxvt_term  r xvt_t;

extern rxvt_t rxvt_current_term;




/* ------------------------------------------------------------------------- */
struct overlay_base
{
//CMNT: c_keyword ^|         int x, y, w, h; // overlay dimensions 
   x, y, w, h; // overlay dimensions
//CMNT: possible_pointer ^|         text_t **text; 
  text_t  * text;
//CMNT: c_keyword possible_pointer ^|         rend_t **rend; 
    * rend;

  // while tempting to add swap() etc. here, it effetcively only increases code size
};

/* ------------------------------------------------------------------------- */

typedef struct
{
//CMNT: c_keyword ^|         int row; 
   row;
//CMNT: c_keyword ^|         int col; 
   col;
} row_col_t;

/*
//CMNT: possible_pointer ^|        * terminal limits: 
    terminal limits:
 *
//CMNT: possible_pointer ^|        *  width      : 1 <= width 
     width      : 1 <= width
//CMNT: possible_pointer ^|        *  height     : 1 <= height 
     height     : 1 <= height
//CMNT: possible_pointer ^|        *  ncol       : 1 <= ncol       <= MAX(tlen_t) 
     ncol       : 1 <= ncol       <= MAX(tlen_t)
//CMNT: possible_pointer remove_casts ^|        *  nrow       : 1 <= nrow       <= MAX(int) 
     nrow       : 1 <= nrow       <= MAX
//CMNT: possible_pointer remove_casts ^|        *  saveLines  : 0 <= saveLines  <= MAX(int) 
     saveLines  : 0 <= saveLines  <= MAX
//CMNT: possible_pointer ^|        *  term_start : 0 <= term_start < saveLines 
     term_start : 0 <= term_start < saveLines
//CMNT: possible_pointer ^|        *  total_rows : nrow + saveLines 
     total_rows : nrow + saveLines
 *
//CMNT: possible_pointer ^|        *  top_row    : -saveLines <= top_row    <= 0 
     top_row    : -saveLines <= top_row    <= 0
//CMNT: possible_pointer ^|        *  view_start : top_row    <= view_start <= 0 
     view_start : top_row    <= view_start <= 0
 *
//CMNT: possible_pointer ^|        *          | most coordinates are stored relative to term_start, 
             | most coordinates are stored relative to term_start,
//CMNT: possible_pointer ^|        *  ROW_BUF | which is the first line of the terminal screen 
     ROW_BUF | which is the first line of the terminal screen
//CMNT: possible_pointer ^|        *  |························= row_buf[0] 
     |························= row_buf[0]
//CMNT: possible_pointer ^|        *  |························= row_buf[1] 
     |························= row_buf[1]
//CMNT: possible_pointer ^|        *  |························= row_buf[2] etc. 
     |························= row_buf[2] etc.
//CMNT: possible_pointer ^|        *  | 
     |
//CMNT: possible_pointer ^|        *  +------------+···········= term_start + top_row 
     +------------+···········= term_start + top_row
//CMNT: possible_pointer ^|        *  | scrollback | 
     | scrollback |
//CMNT: possible_pointer ^|        *  | scrollback +---------+·= term_start + view_start 
     | scrollback +---------+·= term_start + view_start
//CMNT: possible_pointer ^|        *  | scrollback | display | 
     | scrollback | display |
//CMNT: possible_pointer ^|        *  | scrollback | display | 
     | scrollback | display |
//CMNT: possible_pointer ^|        *  +------------+·display·+·= term_start 
     +------------+·display·+·= term_start
//CMNT: possible_pointer ^|        *  |  terminal  | display | 
     |  terminal  | display |
//CMNT: possible_pointer ^|        *  |  terminal  +---------+ 
     |  terminal  +---------+
//CMNT: possible_pointer ^|        *  |  terminal  | 
     |  terminal  |
//CMNT: possible_pointer ^|        *  |  terminal  | 
     |  terminal  |
//CMNT: possible_pointer ^|        *  +------------+···········= term_start + nrow - 1 
     +------------+···········= term_start + nrow - 1
//CMNT: possible_pointer ^|        *  | 
     |
//CMNT: possible_pointer ^|        *  | 
     |
//CMNT: possible_pointer ^|        *  END······················= total_rows 
     END······················= total_rows
 */

struct TermWin_t
{
//CMNT: c_keyword ^|         int width; /* window width                    [pixels] */ 
   width; /* window width                    [pixels] */
//CMNT: c_keyword ^|         int height; /* window height                   [pixels] */ 
   height; /* window height                   [pixels] */
//CMNT: c_keyword ^|         int fwidth; /* font width                      [pixels] */ 
   fwidth; /* font width                      [pixels] */
//CMNT: c_keyword ^|         int fheight; /* font height                     [pixels] */ 
   fheight; /* font height                     [pixels] */
//CMNT: c_keyword ^|         int fbase; /* font ascent (baseline)          [pixels] */ 
   fbase; /* font ascent (baseline)          [pixels] */
//CMNT: c_keyword ^|         int ncol; /* window columns              [characters] */ 
   ncol; /* window columns              [acters] */
//CMNT: c_keyword ^|         int nrow; /* window rows                 [characters] */ 
   nrow; /* window rows                 [acters] */
//CMNT: c_keyword ^|         int focus; /* window has focus                         */ 
   focus; /* window has focus                         */
//CMNT: c_keyword ^|         int mapped; /* window state mapped?                     */ 
   mapped; /* window state mapped?                     */
//CMNT: c_keyword ^|         int int_bwidth; /* internal border width                    */ 
   _bwidth; /* ernal border width                    */
//CMNT: c_keyword ^|         int ext_bwidth; /* external border width                    */ 
   ext_bwidth; /* external border width                    */
//CMNT: c_keyword ^|         int lineSpace; /* number of extra pixels between rows      */ 
   lineSpace; /* number of extra pixels between rows      */
//CMNT: c_keyword ^|         int saveLines; /* number of lines that fit in scrollback   */ 
   saveLines; /* number of lines that fit in scrollback   */
//CMNT: c_keyword ^|         int total_rows; /* total number of rows in this terminal    */ 
   total_rows; /* total number of rows in this terminal    */
//CMNT: c_keyword ^|         int term_start; /* term lines start here                    */ 
   term_start; /* term lines start here                    */
//CMNT: c_keyword ^|         int view_start; /* scrollback view starts here              */ 
   view_start; /* scrollback view starts here              */
//CMNT: c_keyword ^|         int top_row; /* topmost row index of scrollback          */ 
   top_row; /* topmost row index of scrollback          */
  Window parent[6]; /* parent identifiers - we're parent[0]     */
  Window vt; /* vt100 window                             */
  GC gc; /* GC for drawing                           */
  Pixmap pixmap;
//CMNT: possible_pointer ^|         rxvt_drawable *drawable; 
  rxvt_drawable  d rawable;
//CMNT: possible_pointer ^|         rxvt_fontset *fontset[4]; 
  rxvt_fontset  f ontset[4];
};

/*
//CMNT: possible_pointer ^|        * screen accounting: 
    screen accounting:
//CMNT: possible_pointer ^|        * screen_t elements 
    screen_t elements
//CMNT: possible_pointer ^|        *   row:       Cursor row position                   : 0 <= row < nrow 
      row:       Cursor row position                   : 0 <= row < nrow
//CMNT: possible_pointer ^|        *   col:       Cursor column position                : 0 <= col < ncol 
      col:       Cursor column position                : 0 <= col < ncol
//CMNT: possible_pointer ^|        *   tscroll:   Scrolling region top row inclusive    : 0 <= row < nrow 
      tscroll:   Scrolling region top row inclusive    : 0 <= row < nrow
//CMNT: possible_pointer ^|        *   bscroll:   Scrolling region bottom row inclusive : 0 <= row < nrow 
      bscroll:   Scrolling region bottom row inclusive : 0 <= row < nrow
 *
//CMNT: possible_pointer ^|        * selection_t elements 
    selection_t elements
//CMNT: possible_pointer ^|        *   clicks:    1, 2 or 3 clicks - 4 indicates a special condition of 1 where 
      clicks:    1, 2 or 3 clicks - 4 indicates a special condition of 1 where
//CMNT: possible_pointer ^|        *              nothing is selected 
                 nothing is selected
//CMNT: possible_pointer ^|        *   beg:       row/column of beginning of selection  : never past mark 
      beg:       row/column of beginning of selection  : never past mark
//CMNT: possible_pointer ^|        *   mark:      row/column of initial click           : never past end 
      mark:      row/column of initial click           : never past end
//CMNT: c_keyword possible_pointer ^|        *   end:       row/column of one character past end of selection 
      end:       row/column of one acter past end of selection
//CMNT: possible_pointer ^|        * * Note: top_row <= beg.row <= mark.row <= end.row < nrow 
    * Note: top_row <= beg.row <= mark.row <= end.row < nrow
//CMNT: possible_pointer ^|        * * Note: col == -1 ==> we're left of screen 
    * Note: col == -1 ==> we're left of screen
 *
 */
struct screen_t
{
  row_col_t cur; /* cursor position on the screen             */
//CMNT: c_keyword ^|         int tscroll; /* top of settable scroll region             */ 
   tscroll; /* top of settable scroll region             */
//CMNT: c_keyword ^|         int bscroll; /* bottom of settable scroll region          */ 
   bscroll; /* bottom of settable scroll region          */
//CMNT: c_keyword ^|         unsigned int charset; /* character set number [0..3]               */ 
    set; /* acter set number [0..3]               */
//CMNT: c_keyword ^|         unsigned int flags; /* see below                                 */ 
    flags; /* see below                                 */
  row_col_t s_cur; /* saved cursor position                     */
//CMNT: c_keyword ^|         unsigned int s_charset; /* saved character set number [0..3]         */ 
    s_set; /* saved acter set number [0..3]         */
//CMNT: c_keyword ^|         char s_charset_char; 
   s_set_;
//CMNT: c_keyword ^|         rend_t s_rstyle; /* saved rendition style                     */ 
   s_rstyle; /* saved rendition style                     */
};

enum selection_op_t
{
  SELECTION_CLEAR = 0, /* nothing selected                          */
//CMNT: c_keyword ^|         SELECTION_INIT, /* marked a point                            */ 
  SELECTION_INIT, /* marked a po                            */
  SELECTION_BEGIN, /* started a selection                       */
  SELECTION_CONT, /* continued selection                       */
  SELECTION_DONE /* selection put in CUT_BUFFER0              */
};

struct selection_t
{
//CMNT: c_keyword possible_pointer ^|         wchar_t *text; /* selected text                             */ 
    t ext; /* selected text                             */
//CMNT: c_keyword ^|         unsigned int len; /* length of selected text                   */ 
    len; /* length of selected text                   */
//CMNT: c_keyword ^|         unsigned int screen; /* screen being used                         */ 
    screen; /* screen being used                         */
//CMNT: c_keyword ^|         unsigned int clicks; /* number of clicks                          */ 
    clicks; /* number of clicks                          */
  selection_op_t op; /* current operation                         */
  bool rect; /* rectangular selection?                    */
  row_col_t beg; /* beginning of selection   <= mark          */
//CMNT: c_keyword ^|         row_col_t mark; /* point of initial click   <= end           */ 
  row_col_t mark; /* po of initial click   <= end           */
//CMNT: c_keyword ^|         row_col_t end; /* one character past end point              */ 
  row_col_t end; /* one acter past end po              */
};

