/* eventmask, revents, events... */
      //     # 150 "./../libev/ev.h"
/* can be used to add custom fields to all watchers, while losing binary compatibility */
      //     # 168 "./../libev/ev.h"
/*
//CMNT: possible_pointer ^|        * struct member types: 
    struct member types:
//CMNT: possible_pointer ^|        * private: you can look at them, but not change them, and they might not mean anything to you. 
    private: you can look at them, but not change them, and they might not mean anything to you.
//CMNT: possible_pointer ^|        * ro: can be read anytime, but only changed when the watcher isn't active 
    ro: can be read anytime, but only changed when the watcher isn't active
//CMNT: possible_pointer ^|        * rw: can be read and modified anytime, even when the watcher is active 
    rw: can be read and modified anytime, even when the watcher is active
 *
//CMNT: c_keyword possible_pointer ^|        * some internal details that might be helpful for debugging: 
    some ernal details that might be helpful for debugging:
 *
//CMNT: possible_pointer ^|        * active is either 0, which means the watcher is not active, 
    active is either 0, which means the watcher is not active,
//CMNT: possible_pointer ^|        *           or the array index of the watcher (periodics, timers) 
              or the array index of the watcher (periodics, timers)
//CMNT: possible_pointer ^|        *           or the array index + 1 (most other watchers) 
              or the array index + 1 (most other watchers)
//CMNT: possible_pointer ^|        *           or simply 1 for watchers that aren't in some array. 
              or simply 1 for watchers that aren't in some array.
//CMNT: possible_pointer ^|        * pending is either 0, in which case the watcher isn't, 
    pending is either 0, in which case the watcher isn't,
//CMNT: possible_pointer ^|        *            or the array index + 1 in the pendings array. 
               or the array index + 1 in the pendings array.
 */

/* shared by all watchers */
      //     # 200 "./../libev/ev.h"
/* base class, nothing to see here unless you subclass */
typedef struct ev_watcher
{
//CMNT: c_keyword possible_pointer ^|         int active; /* private */ int pending; /* private */ int priority; /* private */ void *data; /* rw */ void (*cb)( struct ev_watcher *w, int revents); /* private */ 
   active; /* private */  pending; /* private */  priority; /* private */  d ata; /* rw */ ( c b)( struct ev_watcher  w ,  revents); /* private */
} ev_watcher;

/* base class, nothing to see here unless you subclass */
typedef struct ev_watcher_list
{
//CMNT: c_keyword possible_pointer ^|         int active; /* private */ int pending; /* private */ int priority; /* private */ void *data; /* rw */ void (*cb)( struct ev_watcher_list *w, int revents); /* private */ struct ev_watcher_list *next; /* private */ 
   active; /* private */  pending; /* private */  priority; /* private */  d ata; /* rw */ ( c b)( struct ev_watcher_list  w ,  revents); /* private */ struct ev_watcher_list  n ext; /* private */
} ev_watcher_list;

/* base class, nothing to see here unless you subclass */
typedef struct ev_watcher_time
{
//CMNT: c_keyword possible_pointer ^|         int active; /* private */ int pending; /* private */ int priority; /* private */ void *data; /* rw */ void (*cb)( struct ev_watcher_time *w, int revents); /* private */ ev_tstamp at; /* private */ 
   active; /* private */  pending; /* private */  priority; /* private */  d ata; /* rw */ ( c b)( struct ev_watcher_time  w ,  revents); /* private */ ev_tstamp at; /* private */
} ev_watcher_time;

/* invoked when fd is either EV_READable or EV_WRITEable */
/* revent EV_READ, EV_WRITE */
typedef struct ev_io
{
//CMNT: c_keyword possible_pointer ^|         int active; /* private */ int pending; /* private */ int priority; /* private */ void *data; /* rw */ void (*cb)( struct ev_io *w, int revents); /* private */ struct ev_watcher_list *next; /* private */ 
   active; /* private */  pending; /* private */  priority; /* private */  d ata; /* rw */ ( c b)( struct ev_io  w ,  revents); /* private */ struct ev_watcher_list  n ext; /* private */

//CMNT: c_keyword ^|         int fd; /* ro */ 
   fd; /* ro */
//CMNT: c_keyword ^|         int events; /* ro */ 
   events; /* ro */
} ev_io;

/* invoked after a specific time, repeatable (based on monotonic clock) */
/* revent EV_TIMEOUT */
typedef struct ev_timer
{
//CMNT: c_keyword possible_pointer ^|         int active; /* private */ int pending; /* private */ int priority; /* private */ void *data; /* rw */ void (*cb)( struct ev_timer *w, int revents); /* private */ ev_tstamp at; /* private */ 
   active; /* private */  pending; /* private */  priority; /* private */  d ata; /* rw */ ( c b)( struct ev_timer  w ,  revents); /* private */ ev_tstamp at; /* private */

  ev_tstamp repeat; /* rw */
} ev_timer;

//CMNT: c_keyword ^|       /* invoked at some specific time, possibly repeating at regular intervals (based on UTC) */ 
/* invoked at some specific time, possibly repeating at regular ervals (based on UTC) */
/* revent EV_PERIODIC */
typedef struct ev_periodic
{
//CMNT: c_keyword possible_pointer ^|         int active; /* private */ int pending; /* private */ int priority; /* private */ void *data; /* rw */ void (*cb)( struct ev_periodic *w, int revents); /* private */ ev_tstamp at; /* private */ 
   active; /* private */  pending; /* private */  priority; /* private */  d ata; /* rw */ ( c b)( struct ev_periodic  w ,  revents); /* private */ ev_tstamp at; /* private */

