/*
//CMNT: possible_pointer ^|          No need to #undef the __*_{8,64}_LENGTH_MODIFIER__ macros; 
   No need to #undef the __ _ {8,64}_LENGTH_MODIFIER__ macros;
   in fact, you can't #undef them, because later uses of any of
//CMNT: c_keyword possible_pointer ^|          their dependents will *not* then do the intended substitution. 
   their dependents will  n ot   then do the ended substitution.
   Expansion of a #define like this one:

        #define x IDENT y

//CMNT: possible_pointer ^|          uses the cpp value of IDENT at the location where x is *expanded*, 
   uses the cpp value of IDENT at the location where x is  e xpanded , 
   not where it is #defined.
*/
      //     # 5 "encoding.h" 2

//CMNT: c_keyword ^|       typedef uint32_t unicode_t; 
typedef u32_t ;

// order must match the table in encoding.C(!)
enum codeset {
  CS_UNKNOWN = 0,
  CS_US_ASCII,

  CS_ISO8859_1,
  CS_ISO8859_2,
  CS_ISO8859_3,
  CS_ISO8859_4,
  CS_ISO8859_5,
  CS_ISO8859_6,
  CS_ISO8859_7,
  CS_ISO8859_8,
  CS_ISO8859_9,
  CS_ISO8859_10,
  CS_ISO8859_11,
  CS_ISO8859_13,
  CS_ISO8859_14,
  CS_ISO8859_15,
  CS_ISO8859_16,

  CS_KOI8_R,
  CS_KOI8_U,
  CS_JIS0201_1976_0,
  CS_JIS0208_1990_0,
  CS_JIS0212_1990_0,
  CS_JIS0213_1,
  CS_JIS0213_2,

  CS_KSC5601_1987_0,

  CS_GB2312_1980_0,
  CS_GBK_0,

  CS_CNS11643_1992_1,
  CS_CNS11643_1992_2,
  CS_CNS11643_1992_3,
  CS_CNS11643_1992_4,
  CS_CNS11643_1992_5,
  CS_CNS11643_1992_6,
  CS_CNS11643_1992_7,
  CS_CNS11643_1992_F,

  CS_BIG5,
  CS_BIG5_EXT,
  CS_BIG5_PLUS,

  CS_VISCII,

  CS_UNICODE_16, /* 16-bit subset of unicode, for X11 */
  CS_UNICODE,

  NUM_CODESETS
};

//CMNT: c_keyword possible_pointer ^|       codeset codeset_from_name (const char *name); 
codeset codeset_from_name (   n ame);

enum {
  ZERO_WIDTH_CHAR = 0x200b,
  REPLACEMENT_CHAR = 0xfffd,
  NOCHAR = 0xffff, // must be invalid in ANY codeset (!)
};

struct rxvt_codeset_conv
{
//CMNT: c_keyword possible_pointer ^|         uint32_t (*from_unicode) (unicode_t unicode); 
  u32_t ( f rom_unicode) (unicode);



};

