  typedef R ( p tr_type)(  s elf, A1);

//CMNT: possible_pointer ^|         template<class K, R (K::*method)(A1)> 
  template<class K, R (K:: m ethod)(A1)>
//CMNT: c_keyword possible_pointer ^|         void set (K *object){ 
 set (K  o bject){
    self = object;
    func = thunk<K, method>;
  }

//CMNT: c_keyword ^|         R call (A1 a1) const 
  R call (A1 a1) 
  {
    return func (self, a1);
  }

//CMNT: c_keyword ^|         R operator ()(A1 a1) const 
  R operator ()(A1 a1) 
  {
    return call (a1);
  }

private:

