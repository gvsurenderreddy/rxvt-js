//CMNT: possible_pointer ^|        *  
    
//CMNT: possible_pointer ^|        * @APPLE_OSREFERENCE_LICENSE_HEADER_END@ 
    @APPLE_OSREFERENCE_LICENSE_HEADER_END@
 */
      //     # 42 "/usr/include/libkern/i386/_OSByteOrder.h" 3 4
/* Generic byte swapping functions. */

//CMNT: c_keyword ^|       static inline 
 inline
//CMNT: c_keyword ^|       __uint16_t 
__u16_t
_OSSwapInt16(
//CMNT: c_keyword ^|           __uint16_t _data 
    __u16_t _data
){
    return ((_data << 8) | (_data >> 8));
}

//CMNT: c_keyword ^|       static inline 
 inline
//CMNT: c_keyword ^|       __uint32_t 
__u32_t
_OSSwapInt32(
//CMNT: c_keyword ^|           __uint32_t _data 
    __u32_t _data
){
    __asm__ ("bswap   %0" : "+r" (_data));
    return _data;
}


//CMNT: c_keyword ^|       static inline 
 inline
//CMNT: c_keyword ^|       __uint64_t 
__u64_t
_OSSwapInt64(
//CMNT: c_keyword ^|           __uint64_t _data 
    __u64_t _data
){
    __asm__ ("bswap   %%eax\n\t"
             "bswap   %%edx\n\t"
             "xchgl   %%eax, %%edx"
             : "+A" (_data));
    return _data;
}
      //     # 67 "/usr/include/libkern/_OSByteOrder.h" 2 3 4
      //     # 125 "/usr/include/sys/_endian.h" 2 3 4
      //     # 100 "/usr/include/i386/endian.h" 2 3 4
      //     # 38 "/usr/include/machine/endian.h" 2 3 4
      //     # 194 "/usr/include/sys/wait.h" 2 3 4

/*
//CMNT: possible_pointer ^|        * Deprecated: 
    Deprecated:
//CMNT: possible_pointer ^|        * Structure of the information in the status word returned by wait4. 
    Structure of the information in the status word returned by wait4.
//CMNT: possible_pointer ^|        * If w_stopval==_WSTOPPED, then the second structure describes 
    If w_stopval==_WSTOPPED, then the second structure describes
//CMNT: possible_pointer ^|        * the information returned, else the first. 
    the information returned, else the first.
 */
union wait {
//CMNT: c_keyword ^|        int w_status; /* used in syscall */ 
  w_status; /* used in syscall */
 /*
//CMNT: possible_pointer ^|       	 * Terminated process status. 
	    Terminated process status.
	 */
 struct {

//CMNT: c_keyword ^|         unsigned int w_Termsig:7, /* termination signal */ 
    w_Termsig:7, /* termination signal */
    w_Coredump:1, /* core dump indicator */
    w_Retcode:8, /* exit code if w_termsig==0 */
    w_Filler:16; /* upper bits filler */







 } w_T;
 /*
//CMNT: possible_pointer ^|       	 * Stopped process status.  Returned 
	    Stopped process status.  Returned
//CMNT: possible_pointer ^|       	 * only for traced children unless requested 
	    only for traced children unless requested
//CMNT: possible_pointer ^|       	 * with the WUNTRACED option bit. 
	    with the WUNTRACED option bit.
	 */
 struct {

//CMNT: c_keyword ^|         unsigned int w_Stopval:8, /* == W_STOPPED if stopped */ 
    w_Stopval:8, /* == W_STOPPED if stopped */
    w_Stopsig:8, /* signal that stopped us */
    w_Filler:16; /* upper bits filler */






 } w_S;
};
