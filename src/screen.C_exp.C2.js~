/* See comments in <machine/_type.h> about __darwin_ct_rune_t. */
extern "C" {
//CMNT: c_keyword ^|       unsigned long ___runetype(__darwin_ct_rune_t); 
 long ___runetype(__darwin_ct_rune_t);
__darwin_ct_rune_t ___tolower(__darwin_ct_rune_t);
__darwin_ct_rune_t ___toupper(__darwin_ct_rune_t);
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isascii(int _c){ 
isascii( _c){
 return ((_c & ~0x7F) == 0);
}
      //     # 164 "/usr/include/ctype.h" 3 4
extern "C" {
//CMNT: c_keyword ^|       int __maskrune(__darwin_ct_rune_t, unsigned long); 
 __maskrune(__darwin_ct_rune_t,  long);
}


//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       __istype(__darwin_ct_rune_t _c, unsigned long _f){ 
__istype(__darwin_ct_rune_t _c,  long _f){



 return (isascii(_c) ? !!(_DefaultRuneLocale.__runetype[_c] & _f)
  : !!__maskrune(_c, _f));

}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ __darwin_ct_rune_t 
 inline /* convert to C++ keyword */ __darwin_ct_rune_t
//CMNT: c_keyword ^|       __isctype(__darwin_ct_rune_t _c, unsigned long _f){ 
__isctype(__darwin_ct_rune_t _c,  long _f){



 return (_c < 0 || _c >= (1 <<8 ) /* Must be a power of 2 */) ? 0 :
  !!(_DefaultRuneLocale.__runetype[_c] & _f);

}
      //     # 204 "/usr/include/ctype.h" 3 4
extern "C" {
__darwin_ct_rune_t __toupper(__darwin_ct_rune_t);
__darwin_ct_rune_t __tolower(__darwin_ct_rune_t);
}


//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
__wcwidth(__darwin_ct_rune_t _c){
//CMNT: c_keyword ^|        unsigned int _x; 
   _x;

 if (_c == 0)
  return (0);
//CMNT: c_keyword ^|        _x = (unsigned int)__maskrune(_c, 0xe0000000L /* Mask for screen width data */|0x00040000L /* Print */); 
 _x = ( )__maskrune(_c, 0xe0000000L /* Mask for screen width data */|0x00040000L /* Pr */);
 if ((_x & 0xe0000000L /* Mask for screen width data */) != 0)
  return ((_x & 0xe0000000L /* Mask for screen width data */) >> 30 /* Bits to shift to get width */);
//CMNT: c_keyword ^|        return ((_x & 0x00040000L /* Print */) != 0 ? 1 : -1); 
 return ((_x & 0x00040000L /* Pr */) != 0 ? 1 : -1);
}






//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isalnum(int _c){ 
isalnum( _c){
 return (__istype(_c, 0x00000100L /* Alpha */|0x00000400L /* Digit */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isalpha(int _c){ 
isalpha( _c){
 return (__istype(_c, 0x00000100L /* Alpha */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isblank(int _c){ 
isblank( _c){
 return (__istype(_c, 0x00020000L /* Blank */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iscntrl(int _c){ 
iscntrl( _c){
 return (__istype(_c, 0x00000200L /* Control */));
}

/* ANSI -- locale independent */
//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isdigit(int _c){ 
isdigit( _c){
 return (__isctype(_c, 0x00000400L /* Digit */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isgraph(int _c){ 
isgraph( _c){
 return (__istype(_c, 0x00000800L /* Graph */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       islower(int _c){ 
islower( _c){
 return (__istype(_c, 0x00001000L /* Lower */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isprint(int _c){ 
ispr( _c){
//CMNT: c_keyword ^|        return (__istype(_c, 0x00040000L /* Print */)); 
 return (__istype(_c, 0x00040000L /* Pr */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       ispunct(int _c){ 
ispunct( _c){
 return (__istype(_c, 0x00002000L /* Punct */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isspace(int _c){ 
isspace( _c){
 return (__istype(_c, 0x00004000L /* Space */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isupper(int _c){ 
isupper( _c){
 return (__istype(_c, 0x00008000L /* Upper */));
}

/* ANSI -- locale independent */
//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       isxdigit(int _c){ 
isxdigit( _c){
 return (__isctype(_c, 0x00010000L /* X digit */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       toascii(int _c){ 
toascii( _c){
 return (_c & 0x7F);
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       tolower(int _c){ 
tolower( _c){
        return (__tolower(_c));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       toupper(int _c){ 
toupper( _c){
        return (__toupper(_c));
}


//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
