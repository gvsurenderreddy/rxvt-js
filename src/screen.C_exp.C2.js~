//CMNT: c_keyword ^|       typedef unsigned char u_char; 
typedef   u_;
//CMNT: c_keyword ^|       typedef unsigned short u_short; 
typedef   u_;
//CMNT: c_keyword ^|       typedef unsigned int u_int; 
typedef   u_;

//CMNT: c_keyword ^|       typedef unsigned long u_long; 
typedef  long u_long;


//CMNT: c_keyword ^|       typedef unsigned short ushort; /* Sys V compatibility */ 
typedef   u; /* Sys V compatibility */
//CMNT: c_keyword ^|       typedef unsigned int uint; /* Sys V compatibility */ 
typedef   u; /* Sys V compatibility */


//CMNT: c_keyword ^|       typedef u_int64_t u_quad_t; /* quads */ 
typedef u_64_t u_quad_t; /* quads */
//CMNT: c_keyword ^|       typedef int64_t quad_t; 
typedef 64_t quad_t;
//CMNT: possible_pointer ^|       typedef quad_t * qaddr_t; 
typedef quad_t    qaddr_t;

//CMNT: c_keyword possible_pointer ^|       typedef char * caddr_t; /* core address */ 
typedef     caddr_t; /* core address */
//CMNT: c_keyword ^|       typedef int32_t daddr_t; /* disk address */ 
typedef 32_t daddr_t; /* disk address */






//CMNT: c_keyword ^|       typedef u_int32_t fixpt_t; /* fixed point number */ 
typedef u_32_t fixpt_t; /* fixed po number */


typedef __darwin_blkcnt_t blkcnt_t;




typedef __darwin_blksize_t blksize_t;




typedef __darwin_gid_t gid_t;





//CMNT: c_keyword ^|       typedef __uint32_t in_addr_t; /* base type for internet address */ 
typedef __u32_t in_addr_t; /* base type for ernet address */




//CMNT: c_keyword ^|       typedef __uint16_t in_port_t; 
typedef __u16_t in_port_t;



typedef __darwin_ino_t ino_t; /* inode number */





typedef __darwin_ino64_t ino64_t; /* 64bit inode number */






//CMNT: c_keyword ^|       typedef __int32_t key_t; /* IPC key (for Sys V IPC) */ 
typedef __32_t key_t; /* IPC key (for Sys V IPC) */
      //     # 157 "/usr/include/sys/types.h" 3 4
//CMNT: c_keyword ^|       typedef __uint16_t nlink_t; /* link count */ 
typedef __u16_t nlink_t; /* link count */
      //     # 176 "/usr/include/sys/types.h" 3 4
//CMNT: c_keyword ^|       typedef int32_t segsz_t; /* segment size */ 
typedef 32_t segsz_t; /* segment size */
//CMNT: c_keyword ^|       typedef int32_t swblk_t; /* swap offset */ 
typedef 32_t swblk_t; /* swap offset */
      //     # 190 "/usr/include/sys/types.h" 3 4
/* Major, minor numbers, dev_t's. */

/*
//CMNT: possible_pointer ^|        * These lowercase macros tend to match member functions in some C++ code, 
    These lowercase macros tend to match member functions in some C++ code,
//CMNT: possible_pointer ^|        * so for C++, we must use inline functions instead. 
    so for C++, we must use inline functions instead.
 */

//CMNT: c_keyword ^|       static inline __int32_t major(__uint32_t _x){ 
 inline __32_t major(__u32_t _x){
//CMNT: c_keyword ^|        return (__int32_t)(((__uint32_t)_x >> 24) & 0xff); 
 return (__32_t)(((__u32_t)_x >> 24) & 0xff);
}

//CMNT: c_keyword ^|       static inline __int32_t minor(__uint32_t _x){ 
 inline __32_t minor(__u32_t _x){
//CMNT: c_keyword ^|        return (__int32_t)((_x) & 0xffffff); 
 return (__32_t)((_x) & 0xffffff);
}

//CMNT: c_keyword ^|       static inline dev_t makedev(__uint32_t _major, __uint32_t _minor){ 
 inline dev_t makedev(__u32_t _major, __u32_t _minor){
 return (dev_t)(((_major) << 24) | (_minor));
}
      //     # 223 "/usr/include/sys/types.h" 3 4
typedef __darwin_clock_t clock_t;
      //     # 235 "/usr/include/sys/types.h" 3 4
typedef __darwin_ssize_t ssize_t;




typedef __darwin_time_t time_t;




typedef __darwin_useconds_t useconds_t;




typedef __darwin_suseconds_t suseconds_t;



