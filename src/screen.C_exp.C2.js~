//CMNT: c_keyword possible_pointer ^|         void *self; 
  s elf;
  ptr_type func;

//CMNT: possible_pointer ^|         template<class klass, R (klass::*method)(A1, A2, A3, A4, A5)> 
  template<class klass, R (klass:: m ethod)(A1, A2, A3, A4, A5)>
//CMNT: c_keyword possible_pointer ^|         static R thunk (void *self, A1 a1, A2 a2, A3 a3, A4 a4, A5 a5){ 
   R thunk (  s elf, A1 a1, A2 a2, A3 a3, A4 a4, A5 a5){
//CMNT: c_keyword possible_pointer ^|           klass *obj = static_cast<klass *>(self); 
