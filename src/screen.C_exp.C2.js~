/*
//CMNT: possible_pointer ^|        * Use inline functions if we are allowed to and the compiler supports them. 
    Use inline functions if we are allowed to and the compiler supports them.
 */



//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswalnum(wint_t _wc){ 
iswalnum(w_t _wc){
 return (__istype(_wc, 0x00000100L /* Alpha */|0x00000400L /* Digit */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswalpha(wint_t _wc){ 
iswalpha(w_t _wc){
 return (__istype(_wc, 0x00000100L /* Alpha */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswcntrl(wint_t _wc){ 
iswcntrl(w_t _wc){
 return (__istype(_wc, 0x00000200L /* Control */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswctype(wint_t _wc, wctype_t _charclass){ 
iswctype(w_t _wc, wctype_t _class){
//CMNT: c_keyword ^|        return (__istype(_wc, _charclass)); 
 return (__istype(_wc, _class));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswdigit(wint_t _wc){ 
iswdigit(w_t _wc){
 return (__isctype(_wc, 0x00000400L /* Digit */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswgraph(wint_t _wc){ 
iswgraph(w_t _wc){
 return (__istype(_wc, 0x00000800L /* Graph */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswlower(wint_t _wc){ 
iswlower(w_t _wc){
 return (__istype(_wc, 0x00001000L /* Lower */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswprint(wint_t _wc){ 
iswpr(w_t _wc){
//CMNT: c_keyword ^|        return (__istype(_wc, 0x00040000L /* Print */)); 
 return (__istype(_wc, 0x00040000L /* Pr */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswpunct(wint_t _wc){ 
iswpunct(w_t _wc){
 return (__istype(_wc, 0x00002000L /* Punct */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswspace(wint_t _wc){ 
iswspace(w_t _wc){
 return (__istype(_wc, 0x00004000L /* Space */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswupper(wint_t _wc){ 
iswupper(w_t _wc){
 return (__istype(_wc, 0x00008000L /* Upper */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ int 
 inline /* convert to C++ keyword */ 
//CMNT: c_keyword ^|       iswxdigit(wint_t _wc){ 
iswxdigit(w_t _wc){
 return (__isctype(_wc, 0x00010000L /* X digit */));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ wint_t 
 inline /* convert to C++ keyword */ w_t
//CMNT: c_keyword ^|       towlower(wint_t _wc){ 
towlower(w_t _wc){
        return (__tolower(_wc));
}

//CMNT: c_keyword ^|       static inline /* convert to C++ keyword */ wint_t 
 inline /* convert to C++ keyword */ w_t
//CMNT: c_keyword ^|       towupper(wint_t _wc){ 
towupper(w_t _wc){
        return (__toupper(_wc));
}
      //     # 176 "/usr/include/_wctype.h" 3 4
extern "C" {
wctype_t
//CMNT: c_keyword possible_pointer ^|        wctype(const char *); 
 wctype(   ) ;
}
//CMNT: c_keyword ^|             //     # 115 "/usr/include/wchar.h" 2 3 4 
      //     # 115 "/usr/include/w.h" 2 3 4

extern "C" {
//CMNT: c_keyword remove_casts ^|       wint_t btowc(int); 
w_t btowc;
//CMNT: c_keyword possible_pointer ^|       wint_t fgetwc(FILE *); 
w_t fgetwc(FILE  ) ;
//CMNT: c_keyword possible_pointer ^|       wchar_t *fgetws(wchar_t * , int, FILE * ); 
  f getws(    , , FILE    );
//CMNT: c_keyword possible_pointer ^|       wint_t fputwc(wchar_t, FILE *); 
w_t fputwc(, FILE  ) ;
//CMNT: c_keyword possible_pointer ^|       int fputws(const wchar_t * , FILE * ); 
 fputws(     , FILE    );
//CMNT: c_keyword possible_pointer ^|       int fwide(FILE *, int); 
 fwide(FILE  ,  );
