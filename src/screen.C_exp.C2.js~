  ev::prepare flush_ev; flush_cb (ev::prepare &w,  revents);
//CMNT: c_keyword ^|         ev::io x_ev ; void x_cb (ev::io &w, int revents); 
  ev::io x_ev ; x_cb (ev::io &w,  revents);
      //     # 235 "rxvttoolkit.h"
//public
//CMNT: possible_pointer ^|         Display *dpy; 
  Display  d py;
//CMNT: c_keyword ^|         int screen; 
   screen;
  Window root;
//CMNT: possible_pointer ^|         rxvt_term *selection_owner; 
  rxvt_term  s election_owner;
  Atom xa[NUM_XA];
  bool is_local;




//CMNT: c_keyword possible_pointer ^|         rxvt_display (const char *id); 
  rxvt_display (   i d);
  XrmDatabase get_resources (bool refresh);
  bool ref_init ();
//CMNT: c_keyword ^|         void ref_next (); 
 ref_next ();
  ~rxvt_display ();

//CMNT: c_keyword ^|         void flush (){ 
 flush (){
    flush_ev.start ();
  }

//CMNT: c_keyword possible_pointer ^|         Atom atom (const char *name); 
  Atom atom (   n ame);
//CMNT: c_keyword possible_pointer ^|         void set_selection_owner (rxvt_term *owner); 
