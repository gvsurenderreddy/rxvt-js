//CMNT: possible_pointer ^|               (*static_cast<watcher *>(w), revents); 
        ( s tatic_cast<watcher  > (w), revents);
    }

    // method callback
//CMNT: c_keyword possible_pointer ^|           template<class K, void (K::*method)(watcher &w, int)> 
    template<class K, (K:: m ethod)(watcher &w, )>
//CMNT: c_keyword possible_pointer ^|           void set (K *object) throw (){ 
 set (K  o bject) throw (){
      set_ (object, method_thunk<K, method>);
    }

    // default method == operator ()
    template<class K>
//CMNT: c_keyword possible_pointer ^|           void set (K *object) throw (){ 
 set (K  o bject) throw (){
//CMNT: js_style_functions ^|             set_ (object, method_thunk<K, &K::operator ()>); 
      set_ (object, method_thunk<K, &K.operator =function()>); 
    }

//CMNT: c_keyword possible_pointer ^|           template<class K, void (K::*method)(watcher &w, int)> 
    template<class K, (K:: m ethod)(watcher &w, )>
