extern "C" {
//CMNT: c_keyword possible_pointer ^|       void *memchr(const void *, int, size_t); 
  m emchr(  ,  , size_t);
//CMNT: c_keyword possible_pointer ^|       int memcmp(const void *, const void *, size_t); 
 memcmp(  ,    ,  size_t);
//CMNT: c_keyword possible_pointer ^|       void *memcpy(void *, const void *, size_t); 
  m emcpy(  ,    ,  size_t);
//CMNT: c_keyword possible_pointer ^|       void *memmove(void *, const void *, size_t); 
  m emmove(  ,    ,  size_t);
//CMNT: c_keyword possible_pointer ^|       void *memset(void *, int, size_t); 
  m emset(  ,  , size_t);

//CMNT: c_keyword possible_pointer ^|       char *stpcpy(char *, const char *); 
  s tpcpy(  ,     ) ;
//CMNT: c_keyword possible_pointer ^|       char *strcasestr(const char *, const char *); 
  s trcasestr(   ,     ) ;

//CMNT: c_keyword possible_pointer ^|       char *strcat(char *, const char *); 
  s trcat(  ,     ) ;
//CMNT: c_keyword possible_pointer ^|       char *strchr(const char *, int); 
  s trchr(   ,  );
//CMNT: c_keyword possible_pointer ^|       int strcmp(const char *, const char *); 
 strcmp(   ,     ) ;
//CMNT: c_keyword possible_pointer ^|       int strcoll(const char *, const char *); 
 strcoll(   ,     ) ;
//CMNT: c_keyword possible_pointer ^|       char *strcpy(char *, const char *); 
  s trcpy(  ,     ) ;
//CMNT: c_keyword possible_pointer ^|       size_t strcspn(const char *, const char *); 
size_t strcspn(   ,     ) ;
//CMNT: c_keyword possible_pointer remove_casts ^|       char *strerror(int) __asm("_" "strerror" "$UNIX2003"); 
  s trerror __asm("_" "strerror" "$UNIX2003");
//CMNT: c_keyword possible_pointer ^|       int strerror_r(int, char *, size_t); 
 strerror_r(,   ,  size_t);
//CMNT: c_keyword possible_pointer ^|       size_t strlen(const char *); 
size_t strlen(   ) ;
//CMNT: c_keyword possible_pointer ^|       char *strncat(char *, const char *, size_t); 
  s trncat(  ,     ,  size_t);
//CMNT: c_keyword possible_pointer ^|       int strncmp(const char *, const char *, size_t); 
 strncmp(   ,     ,  size_t);
//CMNT: c_keyword possible_pointer ^|       char *strncpy(char *, const char *, size_t); 
  s trncpy(  ,     ,  size_t);

//CMNT: c_keyword possible_pointer ^|       char *strnstr(const char *, const char *, size_t); 
  s trnstr(   ,     ,  size_t);

//CMNT: c_keyword possible_pointer ^|       char *strpbrk(const char *, const char *); 
  s trpbrk(   ,     ) ;
//CMNT: c_keyword possible_pointer ^|       char *strrchr(const char *, int); 
  s trrchr(   ,  );
