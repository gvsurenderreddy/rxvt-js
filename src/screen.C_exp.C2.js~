      //     # 31 "/usr/include/sys/_structs.h" 2 3 4
      //     # 25 "/usr/include/_structs.h" 2 3 4
      //     # 70 "/usr/include/time.h" 2 3 4
      //     # 90 "/usr/include/time.h" 3 4
struct tm {
//CMNT: c_keyword ^|        int tm_sec; /* seconds after the minute [0-60] */ 
  tm_sec; /* seconds after the minute [0-60] */
//CMNT: c_keyword ^|        int tm_min; /* minutes after the hour [0-59] */ 
  tm_min; /* minutes after the hour [0-59] */
//CMNT: c_keyword ^|        int tm_hour; /* hours since midnight [0-23] */ 
  tm_hour; /* hours since midnight [0-23] */
//CMNT: c_keyword ^|        int tm_mday; /* day of the month [1-31] */ 
  tm_mday; /* day of the month [1-31] */
//CMNT: c_keyword ^|        int tm_mon; /* months since January [0-11] */ 
  tm_mon; /* months since January [0-11] */
//CMNT: c_keyword ^|        int tm_year; /* years since 1900 */ 
  tm_year; /* years since 1900 */
//CMNT: c_keyword ^|        int tm_wday; /* days since Sunday [0-6] */ 
  tm_wday; /* days since Sunday [0-6] */
//CMNT: c_keyword ^|        int tm_yday; /* days since January 1 [0-365] */ 
  tm_yday; /* days since January 1 [0-365] */
//CMNT: c_keyword ^|        int tm_isdst; /* Daylight Savings Time flag */ 
  tm_isdst; /* Daylight Savings Time flag */
 long tm_gmtoff; /* offset from CUT in seconds */
//CMNT: c_keyword possible_pointer ^|        char *tm_zone; /* timezone abbreviation */ 
   t m_zone; /* timezone abbreviation */
};
      //     # 113 "/usr/include/time.h" 3 4
//CMNT: c_keyword possible_pointer ^|       extern char *tzname[]; 
extern   t zname[];


//CMNT: c_keyword ^|       extern int getdate_err; 
extern  getdate_err;

extern long timezone __asm("_" "timezone" "$UNIX2003");

//CMNT: c_keyword ^|       extern int daylight; 
extern  daylight;

extern "C" {
//CMNT: c_keyword possible_pointer ^|       char *asctime(const struct tm *); 
  a sctime( struct tm  ) ;
//CMNT: remove_casts ^|       clock_t clock(void) __asm("_" "clock" "$UNIX2003"); 
clock_t clock __asm("_" "clock" "$UNIX2003");
//CMNT: c_keyword possible_pointer ^|       char *ctime(const time_t *); 
  c time( time_t  ) ;
double difftime(time_t, time_t);
//CMNT: c_keyword possible_pointer ^|       struct tm *getdate(const char *); 
struct tm  g etdate(   ) ;
//CMNT: c_keyword possible_pointer ^|       struct tm *gmtime(const time_t *); 
struct tm  g mtime( time_t  ) ;
//CMNT: c_keyword possible_pointer ^|       struct tm *localtime(const time_t *); 
struct tm  l ocaltime( time_t  ) ;
//CMNT: possible_pointer ^|       time_t mktime(struct tm *) __asm("_" "mktime" "$UNIX2003"); 
time_t mktime(struct tm  )  __asm("_" "mktime" "$UNIX2003");
//CMNT: c_keyword possible_pointer ^|       size_t strftime(char * , size_t, const char * , const struct tm * ) __asm("_" "strftime" "$UNIX2003"); 
size_t strftime(    , size_t,      ,  struct tm    ) __asm("_" "strftime" "$UNIX2003");
//CMNT: c_keyword possible_pointer ^|       char *strptime(const char * , const char * , struct tm * ) __asm("_" "strptime" "$UNIX2003"); 
  s trptime(     ,      , struct tm    ) __asm("_" "strptime" "$UNIX2003");
//CMNT: possible_pointer ^|       time_t time(time_t *); 
time_t time(time_t  ) ;


//CMNT: c_keyword remove_casts ^|       void tzset(void); 
 tzset;


/* [TSF] Thread safe functions */
//CMNT: c_keyword possible_pointer ^|       char *asctime_r(const struct tm * , char * ); 
  a sctime_r( struct tm    ,     );
//CMNT: c_keyword possible_pointer ^|       char *ctime_r(const time_t *, char *); 
  c time_r( time_t  ,    ) ;
//CMNT: c_keyword possible_pointer ^|       struct tm *gmtime_r(const time_t * , struct tm * ); 
struct tm  g mtime_r( time_t    , struct tm    );
//CMNT: c_keyword possible_pointer ^|       struct tm *localtime_r(const time_t * , struct tm * ); 
struct tm  l ocaltime_r( time_t    , struct tm    );


time_t posix2time(time_t);



//CMNT: c_keyword remove_casts ^|       void tzsetwall(void); 
 tzsetwall;
time_t time2posix(time_t);
//CMNT: c_keyword possible_pointer ^|       time_t timelocal(struct tm * const); 
time_t timelocal(struct tm    );
//CMNT: c_keyword possible_pointer ^|       time_t timegm(struct tm * const); 
time_t timegm(struct tm    );



//CMNT: c_keyword possible_pointer ^|       int nanosleep(const struct timespec *, struct timespec *) __asm("_" "nanosleep" /* nothing */ "$UNIX2003"); 
 nanosleep( struct timespec  ,  struct timespec  )  __asm("_" "nanosleep" /* nothing */ "$UNIX2003");

}
      //     # 52 "/usr/include/c++/4.0.0/ctime" 2 3

// Get rid of those macros defined in <time.h> in lieu of real functions.
      //     # 64 "/usr/include/c++/4.0.0/ctime" 3
namespace std
{
  using ::clock_t;
  using ::time_t;
  using ::tm;

  using ::clock;
  using ::difftime;
  using ::mktime;
  using ::time;
  using ::asctime;
  using ::ctime;
  using ::gmtime;
  using ::localtime;
  using ::strftime;
}
