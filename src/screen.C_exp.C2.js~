      //     # 42 "scrollbar.h"
enum sb_state {
  STATE_IDLE = 1,
  STATE_MOTION,
  STATE_UP,
  STATE_DOWN,
};

struct scrollBar_t
{
//CMNT: possible_pointer ^|         rxvt_term *term; 
  rxvt_term  t erm;
//CMNT: c_keyword ^|         char state; /* scrollbar state                          */ 
   state; /* scrollbar state                          */
//CMNT: c_keyword ^|         char init; /* scrollbar has been initialised           */ 
   init; /* scrollbar has been initialised           */
//CMNT: c_keyword ^|         unsigned int beg; /* slider sub-window begin height           */ 
    beg; /* slider sub-window begin height           */
//CMNT: c_keyword ^|         unsigned int end; /* slider sub-window end height             */ 
    end; /* slider sub-window end height             */
//CMNT: c_keyword ^|         unsigned int top; /* slider top position                      */ 
    top; /* slider top position                      */
//CMNT: c_keyword ^|         unsigned int bot; /* slider bottom position                   */ 
    bot; /* slider bottom position                   */
//CMNT: c_keyword ^|         unsigned int style; /* style: rxvt, xterm, next                 */ 
    style; /* style: rxvt, xterm, next                 */
//CMNT: c_keyword ^|         unsigned int width; /* scrollbar width                          */ 
    width; /* scrollbar width                          */
//CMNT: c_keyword ^|         int shadow; /* scrollbar shadow width                   */ 
   shadow; /* scrollbar shadow width                   */
//CMNT: c_keyword ^|         int last_bot; /* scrollbar last bottom position           */ 
   last_bot; /* scrollbar last bottom position           */
//CMNT: c_keyword ^|         int last_top; /* scrollbar last top position              */ 
   last_top; /* scrollbar last top position              */
//CMNT: c_keyword ^|         int last_state; /* scrollbar last state                     */ 
   last_state; /* scrollbar last state                     */
//CMNT: c_keyword ^|         unsigned char align; 
    align;
  Window win;
  Cursor leftptr_cursor;
//CMNT: c_keyword possible_pointer remove_casts ^|         int (scrollBar_t::*update)(int); 
   (scrollBar_t:: u pdate);
//CMNT: c_keyword possible_pointer ^|         void setup (rxvt_term *); 
 setup (rxvt_term  ) ;
//CMNT: c_keyword ^|         void resize (); 
 resize ();
//CMNT: c_keyword remove_casts ^|         int map (int); 
   map ;
//CMNT: c_keyword remove_casts ^|         int show (int); 
   show ;
//CMNT: c_keyword ^|         void destroy (); 
 destroy ();

//CMNT: c_keyword ^|         bool upButton (int y){ 
  bool upButton ( y){
    if (style == 1)
      return y > end && y <= end + width + 1;
    if (style == 8)
      return y < beg;
    return false;
  }
//CMNT: c_keyword ^|         bool dnButton (int y){ 
  bool dnButton ( y){
    if (style == 1)
      return y > end + width + 1;
    if (style == 8)
      return y > end;
    return false;
  }
//CMNT: c_keyword ^|         unsigned min_height (){ 
   min_height (){
//CMNT: possible_pointer ^|           return style == 1 ? ((19 - (1 * 2) - 1) - (1 * 2)) : 10; 
    return style == 1 ? ((19 - (1    2) - 1) - (1    2)) : 10;
  }
//CMNT: c_keyword ^|         unsigned size (){ 
   size (){
    return end - beg - min_height ();
  }
//CMNT: c_keyword ^|         unsigned total_width (){ 
   total_width (){
//CMNT: possible_pointer ^|           return width + shadow * 2; 
    return width + shadow    2;
  }
      //     # 123 "scrollbar.h"
  GC xscrollbarGC,
                  ShadowGC;



  GC pscrollbarGC;


private:
  // update style dependent data
//CMNT: c_keyword ^|         void update_data (); 
 update_data ();

  // scrollbar-next.C
//CMNT: c_keyword remove_casts ^|         int show_next (int); 
   show_next ;
  // scrollbar-rxvt.C
//CMNT: c_keyword remove_casts ^|         int show_rxvt (int); 
   show_rxvt ;
  // scrollbar-xterm.C
//CMNT: c_keyword remove_casts ^|         int show_xterm (int); 
   show_xterm ;
  // scrollbar-plain.C
//CMNT: c_keyword remove_casts ^|         int show_plain (int); 
   show_plain ;

//CMNT: c_keyword ^|         void init_next (); 
 init_next ();
};





