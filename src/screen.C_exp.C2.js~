// for m >= -n, ensure remainder lies between 0..n-1


// makes dynamically allocated objects zero-initialised
struct zero_initialized
{
//CMNT: c_keyword possible_pointer ^|         void *operator new (size_t s); 
  o perator new (size_t s);
//CMNT: c_keyword possible_pointer ^|         void operator delete (void *p, size_t s); 
 operator delete (  p , size_t s);
};

/* simplevec taken (and heavily modified), from:
 *
//CMNT: possible_pointer ^|        *  MICO --- a free CORBA implementation 
     MICO --- a free CORBA implementation
//CMNT: possible_pointer ^|        *  Copyright (C) 1997-98 Kay Roemer & Arno Puder 
     Copyright (C) 1997-98 Kay Roemer & Arno Puder
 */
template<class T>
struct simplevec
{
//CMNT: possible_pointer ^|           typedef T* iterator; 
    typedef T   iterator;
//CMNT: c_keyword possible_pointer ^|           typedef const T* const_iterator; 
    typedef  T   _iterator;
//CMNT: c_keyword ^|           typedef unsigned long size_type; 
    typedef  long size_type;

private:
    size_type _last, _size;
//CMNT: possible_pointer ^|           T *_buf; 
    T  _ buf;

public:
//CMNT: c_keyword ^|           const_iterator begin () const 
    _iterator begin () 
    {
        return &_buf[0];
    }
    iterator begin (){
        return &_buf[0];
    }
//CMNT: c_keyword ^|           const_iterator end () const 
    _iterator end () 
    {
        return &_buf[_last];
    }
    iterator end (){
        return &_buf[_last];
    }
//CMNT: c_keyword ^|           size_type capacity () const 
    size_type capacity () 
    {
        return _size;
    }
//CMNT: c_keyword ^|           size_type size () const 
    size_type size () 
    {
        return _last;
    }

private:
//CMNT: c_keyword possible_pointer ^|           static T *alloc (size_type n){ 
     T  a lloc (size_type n){
//CMNT: possible_pointer ^|               return (T *)::operator new ((size_t) (n * sizeof (T))); 
        return (T  ) ::operator new ((size_t) (n    sizeof (T)));
    }
//CMNT: c_keyword possible_pointer ^|           static void dealloc (T *buf){ 
     dealloc (T  b uf){
        if (buf)
            ::operator delete (buf);
    }

//CMNT: c_keyword ^|           void reserve (iterator where, size_type n){ 
 reserve (iterator where, size_type n){
        if (_last + n <= _size) {
//CMNT: possible_pointer ^|                   memmove (where+n, where, (end ()-where)*sizeof (T)); 
            memmove (where+n, where, (end ()-where) s izeof (T));
        } else {
            size_type sz = _last+n;
//CMNT: possible_pointer ^|                   sz = (_size == 0) ? max (sz, 5) : max (sz, 2*_size); 
            sz = (_size == 0) ? max (sz, 5) : max (sz, 2 _ size);
//CMNT: possible_pointer ^|                   T *nbuf = alloc (sz); 
            T  n buf = alloc (sz);
            if (_buf) {
//CMNT: possible_pointer ^|                       memcpy (nbuf, begin (), (where-begin ())*sizeof (T)); 
                memcpy (nbuf, begin (), (where-begin ()) s izeof (T));
                memcpy (nbuf + (where-begin ()) + n, where,
//CMNT: possible_pointer ^|                               (end ()-where)*sizeof (T)); 
                        (end ()-where) s izeof (T));
                dealloc (_buf);
            }
            _buf = nbuf;
            _size = sz;
        }
    }

public:
//CMNT: c_keyword ^|           void reserve (size_type sz){ 
 reserve (size_type sz){
        if (_size < sz) {
//CMNT: possible_pointer ^|                   sz = (_size == 0) ? max (sz, 5) : max (sz, 2*_size); 
            sz = (_size == 0) ? max (sz, 5) : max (sz, 2 _ size);
//CMNT: possible_pointer ^|                   T *nbuf = alloc (sz); 
            T  n buf = alloc (sz);
            if (_buf) {
//CMNT: possible_pointer ^|                       memcpy (nbuf, begin (), size ()*sizeof (T)); 
                memcpy (nbuf, begin (), size () s izeof (T));
                dealloc (_buf);
            }
            _buf = nbuf;
            _size = sz;
        }
    }
    simplevec ()
    : _last(0), _size(0), _buf(0){
    }
//CMNT: c_keyword ^|           simplevec (size_type n, const T& t = T ()) 
    simplevec (size_type n,  T& t = T ())
    : _last(0), _size(0), _buf(0){
        insert (begin (), n, t);
    }
//CMNT: c_keyword ^|           simplevec (const_iterator first, const_iterator last) 
    simplevec (_iterator first, _iterator last)
    : _last(0), _size(0), _buf(0){
        insert (begin (), first, last);
    }
//CMNT: c_keyword ^|           simplevec (const simplevec<T> &v) 
    simplevec ( simplevec<T> &v)
    : _last(0), _size(0), _buf(0){
        reserve (v._last);
//CMNT: possible_pointer ^|               memcpy (_buf, v.begin (), v.size ()*sizeof (T)); 
        memcpy (_buf, v.begin (), v.size () s izeof (T));
        _last = v._last;
    }
//CMNT: c_keyword ^|           simplevec<T> &operator= (const simplevec<T> &v){ 
    simplevec<T> &operator= ( simplevec<T> &v){
        if (this != &v) {
            _last = 0;
            reserve (v._last);
//CMNT: possible_pointer ^|                   memcpy (_buf, v.begin (), v.size ()*sizeof (T)); 
            memcpy (_buf, v.begin (), v.size () s izeof (T));
            _last = v._last;
        }
//CMNT: possible_pointer ^|               return *this; 
        return  t his;
    }
    ~simplevec (){
        dealloc (_buf);
    }
//CMNT: c_keyword ^|           const T &front () const 
     T &front () 
    {
        //ministl_assert (size () > 0);
        return _buf[0];
    }
    T &front (){
        //ministl_assert (size () > 0);
        return _buf[0];
    }
//CMNT: c_keyword ^|           const T &back () const 
     T &back () 
    {
        //ministl_assert (size () > 0);
        return _buf[_last-1];
    }
    T &back (){
        //ministl_assert (size () > 0);
        return _buf[_last-1];
    }
//CMNT: c_keyword ^|           bool empty () const 
    bool empty () 
    {
        return _last == 0;
    }
//CMNT: c_keyword ^|           void clear (){ 
 clear (){
        _last = 0;
    }
//CMNT: c_keyword ^|           void push_back (const T &t){ 
 push_back ( T &t){
        reserve (_last+1);
//CMNT: possible_pointer ^|               *end () = t; 
         e nd () = t;
        ++_last;
    }
//CMNT: c_keyword ^|           void push_back (T &t){ 
 push_back (T &t){
        reserve (_last+1);
//CMNT: possible_pointer ^|               *end () = t; 
         e nd () = t;
        ++_last;
    }
//CMNT: c_keyword ^|           void pop_back (){ 
 pop_back (){
        //ministl_assert (size () > 0);
        --_last;
    }
//CMNT: c_keyword ^|           const T &operator[] (size_type idx) const 
     T &operator[] (size_type idx) 
    {
        //ministl_assert (idx < size ());
        return _buf[idx];
    }
    T &operator[] (size_type idx){
        //ministl_assert (idx < size ());
        return _buf[idx];
    }
//CMNT: c_keyword ^|           iterator insert (iterator pos, const T &t){ 
    iterator insert (iterator pos,  T &t){
        //ministl_assert (pos <= end ());
        long at = pos - begin ();
        reserve (pos, 1);
        pos = begin ()+at;
//CMNT: possible_pointer ^|               *pos = t; 
         p os = t;
        ++_last;
        return pos;
    }
//CMNT: c_keyword ^|           iterator insert (iterator pos, const_iterator first, const_iterator last){ 
    iterator insert (iterator pos, _iterator first, _iterator last){
        //ministl_assert (pos <= end ());
        long n = last - first;
        long at = pos - begin ();
        if (n > 0) {
            reserve (pos, n);
            pos = begin ()+at;
//CMNT: possible_pointer ^|                   memcpy (pos, first, (last-first)*sizeof (T)); 
            memcpy (pos, first, (last-first) s izeof (T));
            _last += n;
        }
        return pos;
    }
//CMNT: c_keyword ^|           iterator insert (iterator pos, size_type n, const T &t){ 
    iterator insert (iterator pos, size_type n,  T &t){
        //ministl_assert (pos <= end ());
        long at = pos - begin ();
        if (n > 0) {
            reserve (pos, n);
            pos = begin ()+at;
//CMNT: c_keyword ^|                   for (int i = 0; i < n; ++i) 
            for ( i = 0; i < n; ++i)
                pos[i] = t;
            _last += n;
        }
        return pos;
    }
//CMNT: c_keyword ^|           void erase (iterator first, iterator last){ 
 erase (iterator first, iterator last){
        if (last != first) {
//CMNT: possible_pointer ^|                   memmove (first, last, (end () - last) * sizeof (T)); 
            memmove (first, last, (end () - last)    sizeof (T));
            _last -= last - first;
        }
    }
//CMNT: c_keyword ^|           void erase (iterator pos){ 
 erase (iterator pos){
        if (pos != end ()) {
//CMNT: possible_pointer ^|                   memmove (pos, pos+1, (end () - (pos+1)) * sizeof (T)); 
            memmove (pos, pos+1, (end () - (pos+1))    sizeof (T));
            --_last;
        }
    }
//CMNT: c_keyword ^|           void swap (simplevec<T> &t){ 
 swap (simplevec<T> &t){
        ::swap(_last, t._last);
        ::swap(_size, t._size);
        ::swap(_buf, t._buf);
    }
};

template<class T>
//CMNT: c_keyword ^|       bool operator== (const simplevec<T> &v1, const simplevec<T> &v2){ 
bool operator== ( simplevec<T> &v1,  simplevec<T> &v2){
    if (v1.size () != v2.size ())
        return false;
//CMNT: possible_pointer ^|           return !v1.size () || !memcmp (&v1[0], &v2[0], v1.size ()*sizeof (T)); 
    return !v1.size () || !memcmp (&v1[0], &v2[0], v1.size () s izeof (T));
}

template<class T>
//CMNT: c_keyword ^|       bool operator< (const simplevec<T> &v1, const simplevec<T> &v2){ 
bool operator< ( simplevec<T> &v1,  simplevec<T> &v2){
//CMNT: c_keyword ^|           unsigned long minlast = min (v1.size (), v2.size ()); 
     long minlast = min (v1.size (), v2.size ());
//CMNT: c_keyword ^|           for (unsigned long i = 0; i < minlast; ++i) { 
    for ( long i = 0; i < minlast; ++i) {
        if (v1[i] < v2[i])
            return true;
        if (v2[i] < v1[i])
            return false;
    }
    return v1.size () < v2.size ();
}


template<typename T>
struct vector : simplevec<T>
{
};

//CMNT: c_keyword possible_pointer ^|       struct stringvec : simplevec<char *> 
struct stringvec : simplevec<  > 
{
  ~stringvec (){
//CMNT: c_keyword possible_pointer ^|           for (char **c = begin (); c != end (); c++) 
    for (  * c = begin (); c != end (); c++)
//CMNT: possible_pointer ^|             free (*c); 
      free ( c );
  }
};
      //     # 409 "rxvtutil.h"
template<typename T>
struct auto_ptr
{
//CMNT: possible_pointer ^|         T *p; 
  T  p ;

  auto_ptr () : p (0) { }
//CMNT: possible_pointer ^|         auto_ptr (T *a) : p (a) { } 
  auto_ptr (T  a ) : p (a) { }

  auto_ptr (auto_ptr<T> &a){
    p = a.p;
    a.p = 0;
  }

  template<typename A>
  auto_ptr (auto_ptr<A> &a){
    p = a.p;
    a.p = 0;
  }

  ~auto_ptr (){
    delete p;
  }

//CMNT: c_keyword ^|         // void because it makes sense in our context 
  // because it makes sense in our context
//CMNT: js_style_variables possible_pointer ^|         void operator = (T *a){ 
 var operator= (T  a ){ 
    delete p;
    p = a;
  }

//CMNT: js_style_variables ^|         void operator = (auto_ptr &a){ 
 var operator= (auto_ptr &a){ 
//CMNT: possible_pointer ^|           *this = a.p; 
     t his = a.p;
    a.p = 0;
  }

  template<typename A>
//CMNT: js_style_variables ^|         void operator = (auto_ptr<A> &a){ 
 var operator= (auto_ptr<A> &a){ 
//CMNT: possible_pointer ^|           *this = a.p; 
     t his = a.p;
    a.p = 0;
  }

//CMNT: c_keyword possible_pointer ^|         operator T * () const { return p; } 
  operator T    ()  { return p; }

//CMNT: c_keyword possible_pointer ^|         T *operator -> () const { return p; } 
  T  o perator -> ()  { return p; }
//CMNT: c_keyword possible_pointer ^|         T &operator * () const { return *p; } 
  T &operator    ()  { return  p ; }

//CMNT: possible_pointer ^|         T *get (){ 
  T  g et (){
//CMNT: possible_pointer ^|           T *r = p; 
    T  r  = p;
    p = 0;
    return r;
  }
};

//CMNT: c_keyword ^|       typedef auto_ptr<char> auto_str; 
typedef auto_ptr<> auto_str;
